    coroutine.wrap(MXUZTNG_fake_script)()
    local function FLSB_fake_script() -- Chat_C.TitleBar.TweenDrag 
        local script = Instance.new('LocalScript', Chat_C.TitleBar)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local DoDrag = false
        function dragify(Frame)
        dragSpeed = .1
        dragToggle = nil
        dragInput = nil
        dragStart = nil
        
        function updateInput(input)
        if DoDrag == true then
        Delta = input.Position - dragStart
        Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
        game:GetService("TweenService"):Create(Frame, TweenInfo.new(dragSpeed), {Position = Position}):Play()
        end
        end
        
        Frame.InputBegan:Connect(function(input)
        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
        dragToggle = true
        dragStart = input.Position
        startPos = Frame.Position
        input.Changed:Connect(function()
        if (input.UserInputState == Enum.UserInputState.End) then
        dragToggle = false
        end
        end)
        end
        end)
        
        Frame.InputChanged:Connect(function(input)
        if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        dragInput = input
        end
        end)
        
        game:GetService("UserInputService").InputChanged:Connect(function(input)
        if (input == dragInput and dragToggle) then
        updateInput(input)
        end
        end)
        end
        
        dragify(script.Parent.Parent)
        
        local var = false
        
        local UIS = game:GetService("UserInputService")
        UIS.InputBegan:Connect(function(a,gp)
            if gp then
                if a.UserInputType == Enum.UserInputType.MouseButton1 then
                    if DoDrag == true then
                        var = true
                    end
                end
            end
        end)
        UIS.InputEnded:Connect(function(a,gp)
            if gp then
                if a.UserInputType == Enum.UserInputType.MouseButton1 then
                    var = false
                end
            end
        end)
        
        script.Parent.MouseEnter:Connect(function()
            var = true
            DoDrag = true
        end)
        script.Parent.MouseLeave:Connect(function()
            repeat wait() until var == false
            DoDrag = false
        end)
    end
    coroutine.wrap(FLSB_fake_script)()
    local function SLAAP_fake_script() -- Chat_C.Icon.LocalScript 
        local script = Instance.new('LocalScript', Chat_C.Icon)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local RANDOMNO = 3 --math.random(1,30)
        
        local Wait = 1
        
        local function Tween(number)
            local TS = game:GetService("TweenService")
            local A = TweenInfo.new(
                Wait,
                Enum.EasingStyle.Sine,
                Enum.EasingDirection.InOut,
                0,
                false,
                0
            )
            local B = {
                Rotation = number
            }
            TS:Create(script.Parent,A,B):Play()
        end
        local t = false
        while wait((Wait)+(.001)) do
            if t == true then
                t = false
                Tween(20)
            elseif t == false then
                t = true
                Tween(-20)
            end
        end
    end
    coroutine.wrap(SLAAP_fake_script)()
    local function QVJXDT_fake_script() -- Chat_C.Main.ClientHandler 
        local script = Instance.new('LocalScript', Chat_C.Main)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local C2S = script.Parent.C2STranfer
        ----
        local TransitionFrame = script.Parent.InteractionMenu.TransitionFrame
        local CoverSize = UDim2.new(1,0,1,0)
        local CoverPosition = UDim2.new(0,0,0,0)
        local ClosedSize = UDim2.new(0,0,0,0)
        local ClosedPosition = UDim2.new(0.5, 0,0.5, 0)
        ----
        function Open(Name)
            for i,v in pairs(script.Parent.InteractionMenu.sMain:GetChildren())do
                if v:IsA("Frame") or v:IsA("ScrollingFrame") then
                    if v.Name ~= Name then
                        v.Visible = false
                    elseif v.Name == Name then
                        v.Visible = true
                    end
                end
            end
        end
        ----
        local TS = game:GetService("TweenService")
        local db = true
        C2S.OnClientInvoke = function(arg,beVis)
            if arg == 'Transition' then
                if db == true then
                    db = false
                    TS:Create(TransitionFrame,TweenInfo.new(1/2),{Position = CoverPosition}):Play()
                    TS:Create(TransitionFrame,TweenInfo.new(1/2),{Size = CoverSize}):Play()
                    wait(.501)
                    Open(beVis)
                    script.Parent.Sounds.ButtonAppear:Play()
                    TS:Create(TransitionFrame,TweenInfo.new(1/2),{Position = ClosedPosition}):Play()
                    TS:Create(TransitionFrame,TweenInfo.new(1/2),{Size = ClosedSize}):Play()
                    wait(.501)
                    db = true
                end
            end
        end
    end
    coroutine.wrap(QVJXDT_fake_script)()
    local function QRTLP_fake_script() -- Chat_C.Main.ServerHandler 
        local script = Instance.new('Script', Chat_C.Main)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local ClientTranfer = script.Parent.C2STranfer
        local FunctionsFromTabs = script.Parent.TabsHandlerFunc
        local PopupForClient = script.Parent.PopupWarning
        
        local WarningMenu = script.Parent.InteractionMenu.sMain.Warning
        WarningMenu.Visible = false
        
        local Player = script:FindFirstAncestorWhichIsA("Player")
        local Game = {
            Chat = game:GetService("Chat");
            Players = game:GetService("Players");
        }
        local Selected = {}
        
        local sBanned = {}
        
        local PlayerList = script.Parent.InteractionMenu.sMain.Select.PlayerList
        
        local RainingTacos = false
        ---
        local AntiSkid = false
        local AntiChat = false
        local SpawnR6 = false
        ---
        local ServerLock = false
        
        local Owner = script:FindFirstAncestorWhichIsA("Player")
        
        Owner.CharacterAdded:Connect(function(c)
            if c:FindFirstChild("UpperTorso") then
                if SpawnR6 == true then
                    require(script["R6"]):R6(Owner.Name)
                end
            end
        end)
        
        game:GetService("Players").PlayerAdded:Connect(function(p)
            spawn(function()
                if AntiChat == true then
                    repeat wait() until p:FindFirstChild'PlayerGui'
                    local Chat = script.Parent.ToClientScripts.Chat_Custom
                    Chat.Parent = p:FindFirstChild'PlayerGui'
                    Chat.TheChat.Disabled = false
                end
            end)
            spawn(function()
                repeat wait() until p:FindFirstChildWhichIsA("PlayerGui")
                local sban = false
                for i = 1,#sBanned do
                    if p.Name == sBanned[i] then
                        sban = true
                    end
                end
                if sban == true then
                    p:Kick()
                end
            end)
        end)
        
        function FindDes(p,name)
            for i,v in pairs(p:GetDescendants())do
                if v.Name == name then
                    return v
                end
            end
        end
        
        function DoAnti()
        for i,p in pairs(game:GetService("Players"):GetPlayers())do
            if AntiChat == true then
                repeat wait() until p:FindFirstChild'PlayerGui'
                local Chat = script.Parent.ToClientScripts.Chat_C:Clone()
                Chat.Name = 'Chat_Custom'
                Chat.Parent = p:FindFirstChild'PlayerGui'
                Chat.TheChat.Disabled = false
            end
        end
        end
        function RemoveAnti()
        for i,p in pairs(game:GetService("Players"):GetPlayers())do
            if AntiChat == false then
                repeat wait() until p:FindFirstChild'PlayerGui'
                if FindDes(p,"Chat_Custom") then
                    FindDes(p,"Chat_Custom"):Destroy()
                end
            end
        end
        end
        
        game:GetService("Players").PlayerAdded:Connect(function(p)
            if AntiSkid == true then
                if p.AccountAge < 10 then
                    p:Kick("\n{RoXploit}: Sorry, it seems your account may be an alt.")
                end
            end
            if ServerLock == true then
                spawn(function()
                    repeat wait() until p.Character
                    p:Kick("\n{RoXploit}: This server is locked.")
                end)
            end
        end)
        
        spawn(function()
            for i = 1,5 do
                spawn(function()
                    while true do
                        if RainingTacos == true then
                            local p = Instance.new("Part")
                            p.BrickColor = BrickColor.new("Bright blue")
                            p.Parent = workspace.Terrain
                            p.Shape = 0
                            p.Orientation = Vector3.new(math.random(1,50),math.random(1,10),math.random(1,50))
                            local S = Instance.new("SpecialMesh",p)
                            S.TextureId = 'http://www.roblox.com/asset/?id=14846834'
                            S.MeshId = 'http://www.roblox.com/asset/?id=14846869'
                            S.Scale = Vector3.new(10*5,10*5,10*5)
                            p.Size = Vector3.new(1,1,1)
                            p.Locked = true
                            p.Anchored = false
                            p.CanCollide = false
                            p.Position = Vector3.new((math.random(-1000, 1000)),100*2,(math.random(-1000,1000)))
                            wait(0)
                        else
                            wait()
                        end
                    end
                end)
            end
        end)
        
        function Flood()
            local region = Region3.new(Vector3.new(-1250,-50,-1250), Vector3.new(1250,150,1250))
            region = region:ExpandToGrid(4)
            workspace.Terrain:FillRegion(region, 4, Enum.Material.Water)
            workspace.Terrain.WaterReflectance = 1
            workspace.Terrain.WaterTransparency = 1
            workspace.Terrain.WaterWaveSpeed = 10
            workspace.Terrain.WaterWaveSize = 1
        end
        
        function Btools(plr,bool)
            if bool == true then
                require(script.Btools)(plr.Name)
            elseif bool == false then
                for i,v in pairs(plr.Backpack:GetDescendants())do
                    if v:IsA("Tool") then
                        if v.Name:find'Clone' then
                            v:Destroy()
                        elseif v.Name:find('Game') then
                            v:Destroy()
                        elseif v.Name:find('Hammer') then
                            v:Destroy()
                        end
                    end
                end
                for i,v in pairs(plr.Character:GetDescendants())do
                    if v:IsA("Tool") then
                        if v.Name:find'Clone' then
                            v:Destroy()
                        elseif v.Name:find('Game') then
                            v:Destroy()
                        elseif v.Name:find('Hammer') then
                            v:Destroy()
                        end
                    end
                end
            end
        end
        
        function InvisPlayer(Char,bool)
            if bool == true then
                for i,v in pairs(Char:GetDescendants())do
                    if v:IsA("BasePart") then
                        v.Transparency = 1
                        if v.Name == 'Head' then
                            v:WaitForChild'face'.Transparency = 1
                        end
                    end
                end
            elseif bool == false then
                for i,v in pairs(Char:GetDescendants())do
                    if v:IsA("BasePart") then
                        if v.Name ~= "HumanoidRootPart" then
                            v.Transparency = 0
                            if v.Name == 'Head' then
                                v:WaitForChild'face'.Transparency = 0
                            end
                        end
                    end
                end
            end
        end
        
        PlayerList.ChildAdded:Connect(function(button)
            if button:IsA("TextButton") then
                local BoolArg = button:WaitForChild("IsSelected")
                local UserName = button:WaitForChild("Username")
                BoolArg.Changed:Connect(function()
                    if BoolArg.Value == true then
                        table.insert(Selected,UserName.Value)
                    elseif BoolArg.Value == false then
                        for i = 0,#Selected do
                            if Selected[i] == UserName.Value then
                                table.remove(Selected,i)
                            end
                        end
                    end
                end)
            end
        end)
        
        game:GetService("Players").PlayerRemoving:Connect(function(button)
            for i,v in pairs(Selected)do
                if v == button.Name then
                    table.remove(Selected,i)
                end
            end
        end)
        
        function GetPlayers()
            local t = {}
            for i,v in pairs(Selected)do
                for i,c in pairs(game:GetService("Players"):GetPlayers())do
                    if v == c.Name then
                        table.insert(t,c.Name)
                    end
                end
            end
            return t
        end
        
        ClientTranfer.OnServerInvoke = function(p,arg)
            ClientTranfer:InvokeClient(p,'Transition',arg)
        end
        
        FunctionsFromTabs.OnServerInvoke = function(p,TabName,Thing,Val)
            if TabName == 'Player' or TabName == 'Players' then
                if Thing == "Char" then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        local cf = v.Character.HumanoidRootPart.CFrame
                        v.CharacterAppearanceId = Val
                        v:LoadCharacter()
                        for i = 1,5 do
                            game:GetService("RunService").Heartbeat:Wait()
                        end
                        v.Character.HumanoidRootPart.CFrame = cf
                    end
                    end
                    end
                elseif Thing == "Chat" then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        Game.Chat:Chat(v.Character.Head, Val, "White")
                    end
                    end
                    end
                elseif Thing == 'JPower' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        --Stuff
                        v.Character:FindFirstChildWhichIsA('Humanoid').JumpPower = tonumber(Val)
                    end
                    end
                    end
                elseif Thing == 'Speed' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        --Stuff
                        v.Character:FindFirstChildWhichIsA('Humanoid').WalkSpeed = tonumber(Val)
                    end
                    end
                    end
                elseif Thing == 'Health' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        --Stuff
                        local Humanoid = v.Character:FindFirstChildWhichIsA("Humanoid")
                        if tonumber(Val) > Humanoid.MaxHealth then
                            Humanoid.MaxHealth = tonumber(Val)
                            Humanoid.Health = Humanoid.MaxHealth
                        else
                            Humanoid.Health = tonumber(Val)
                        end
                    end
                    end
                    end
                elseif Thing == 'Ban' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        --Stuff
                        script.Parent.SendClientData:InvokeClient(Player,v.Name)
                        if Val ~= nil or Val ~= ' ' then
                            v:Kick("\n{RoXploit}: "..Val)
                        else
                            v:Kick("\n{RoXploit}: You've been serverbanned.")
                        end
                    end
                    end
                    end
                elseif Thing == "R6" then
                    --require(5070375005):R6(PlayersName)
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        require(script["R6"]):R6(v.Name)
                    end
                    end
                    end
                elseif Thing == "Respawn" then
                    --require(5070375005):R6(PlayersName)
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        v:LoadCharacter()
                    end
                    end
                    end
                elseif Thing == "Lag" then
                    --require(5070375005):R6(PlayersName)
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        local LagClient = script.Parent.ToClientScripts.Lag:Clone()
                        LagClient.Parent = v.PlayerGui
                        LagClient.Disabled = false
                    end
                    end
                    end
                elseif Thing == "Explode" then
                    --require(5070375005):R6(PlayersName)
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        local Q = Instance.new("Explosion",v.Character)
                        Q.Position = v.Character.HumanoidRootPart.CFrame.Position
                        v.Character:BreakJoints()
                    end
                    end
                    end
                elseif Thing == "ForceField" and Val == true then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        local Q = Instance.new("ForceField")
                        spawn(function()
                            for i = 1,2 do
                                game:GetService("RunService").Stepped:Wait()
                            end
                            Q.Parent = v.Character
                        end)
                    end
                    end
                    end
                elseif Thing == "ForceField" and Val == false then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        for i,z in pairs(v.Character:GetChildren())do
                            if z:IsA("ForceField") then
                                z:Destroy()
                            end
                        end
                    end
                    end
                    end
                elseif Thing == "Invisible" and Val == true then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        InvisPlayer(v.Character,true)
                    end
                    end
                    end
                elseif Thing == "Invisible" and Val == false then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        InvisPlayer(v.Character,false)
                    end
                    end
                    end
                elseif Thing == "Btools" and Val == true then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        Btools(v,true)
                    end
                    end
                    end
                elseif Thing == "Btools" and Val == false then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        Btools(v,false)
                    end
                    end
                    end
                elseif Thing == "TpToMe" then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        v.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(-2,0,0)
                    end
                    end
                    end
                elseif Thing == "TpToThem" then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        Player.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(-2,0,0)
                    end
                    end
                    end
                elseif Thing == "Kill" then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        v.Character:BreakJoints()
                    end
                    end
                    end
                end
            elseif TabName == 'Server' then
                if Thing == 'ClearWS' then
                    for i,v in pairs(workspace:GetDescendants())do
                        pcall(function()
                            v:Destroy()
                        end)
                    end
                elseif Thing == 'Flood' then
                    Flood()
                elseif Thing == 'ClearTerrain' then
                    workspace.Terrain:Clear()
                elseif Thing == 'Shutdown' then
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                        if Val == "" then
                            v:Kick("\n{RoXploit rewrite}: This server has been shutdown.")
                        else
                            v:Kick("\n{RoXploit rewrite}: "..Val)
                        end
                    end
                elseif Thing == 'ColorSpam' then
                    spawn(function()
                        while wait(.1) do
                            local L = game:GetService("Lighting")
                            L.FogEnd = 100
                            game:GetService("TweenService"):Create(L,TweenInfo.new(.1),{FogColor = BrickColor.Random().Color}):Play()
                        end
                    end)
                elseif Thing == 'MakeBase' then
                    spawn(function()
                        local L = Instance.new("Part")
                        L.Parent = workspace
                        L.Size = Vector3.new(10000,10,10000)
                        L.Anchored = true
                        L.Name = "Base"
                        L.BrickColor = BrickColor.new("Slime green")
                        L.CFrame = CFrame.new(0,13,0)
                    end)
                elseif Thing == "Unanchor" then
                    for i,v in pairs(workspace:GetChildren())do
                        pcall(function()
                            v.Anchored = Val
                        end)
                    end
                elseif Thing == "RainTacos" then
                    RainingTacos = not Val
                elseif Thing == "slock" then
                    RainingTacos = not Val
                end
            elseif TabName == 'Lighting' then
                if Thing == 'Ambient' then
                    local function rRGB(text)
                        local R = text:lower():split', '[1]
                        local G = text:lower():split', '[2]
                        local B = text:lower():split', '[3]
                        return R,G,B
                    end
                    local R,G,B = rRGB(Val)
                    game:GetService("Lighting").Ambient = Color3.fromRGB(R,G,B)
                elseif Thing == 'FogColor' then
                    local function rRGB(text)
                        local R = text:lower():split','[1]
                        local G = text:lower():split','[2]
                        local B = text:lower():split','[3]
                        return R,G,B
                    end
                    local R,G,B = rRGB(Val)
                    game:GetService("Lighting").FogColor = Color3.fromRGB(R,G,B)
                elseif Thing == 'FogEnd' then
                    game:GetService("Lighting").FogEnd = Val
                elseif Thing == 'FogStart' then
                    game:GetService("Lighting").FogStart = Val
                elseif Thing == 'TimeOfDay' then
                    game:GetService("Lighting").TimeOfDay = Val
                end
            elseif TabName == 'Decal' then
                if Thing == 'Spam' then
                    for i,v in pairs(workspace:GetDescendants())do
                        local Decal_Ins = Instance.new("Decal")
                        Decal_Ins.Name = "rxDecal"
                        Decal_Ins.Texture = 'rbxassetid://'..Val
                        pcall(function()
                            for i = 1,6 do
                                local Decal = Decal_Ins:Clone()
                                Decal.Parent = v
                                if i == 1 then
                                    Decal.Face = Enum.NormalId.Top
                                elseif i == 2 then
                                    Decal.Face = Enum.NormalId.Right
                                elseif i == 3 then
                                    Decal.Face = Enum.NormalId.Left
                                elseif i == 4 then
                                    Decal.Face = Enum.NormalId.Front
                                elseif i == 5 then
                                    Decal.Face = Enum.NormalId.Back
                                elseif i == 6 then
                                    Decal.Face = Enum.NormalId.Bottom
                                end
                            end
                        end)
                    end
                elseif Thing == 'Clear' then
                    for i,v in pairs(workspace:GetDescendants())do
                        pcall(function()
                            if v.Name == 'rxDecal' then
                                if v:IsA("Decal") then
                                    v:Destroy()
                                end
                            end
                        end)
                    end
                elseif Thing == 'Skybox' then
                    pcall(function()
                        local L = 'rbxassetid://'..Val
                        if game:GetService("Lighting"):FindFirstChildWhichIsA("Sky") then
                            game:GetService("Lighting"):FindFirstChildWhichIsA("Sky"):Destroy()
                        end
                        local SKB = Instance.new("Sky")
                        SKB.Parent = game:GetService("Lighting")
                        SKB.SkyboxUp = L
                        SKB.SkyboxBk = L
                        SKB.SkyboxDn = L
                        SKB.SkyboxFt = L
                        SKB.SkyboxLf = L
                        SKB.SkyboxRt = L
                    end)
                elseif Thing == 'No Skybox' then
                    pcall(function()
                        if game:GetService("Lighting"):FindFirstChildWhichIsA("Sky") then
                            game:GetService("Lighting"):FindFirstChildWhichIsA("Sky"):Destroy()
                        end
                    end)
                end
            elseif TabName == 'Meme' then
                if Thing == 'Meme' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        if v.Character.Head:FindFirstChild'Meme' then
                            v.Character.Head:FindFirstChild'Meme':Destroy()
                        end
                        local MemeBG = script.Parent.ToClientScripts.Meme:Clone()
                        MemeBG:WaitForChild'ImageLabel'.Image = 'rbxassetid://'..Val
                        MemeBG.Parent = v.Character.Head
                        MemeBG.Adornee = v.Character.Head
                        MemeBG.Enabled = true
                    end
                    end
                    end
                elseif Thing == 'Remove Meme' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        if v.Character.Head:FindFirstChild'Meme' then
                            v.Character.Head:FindFirstChild'Meme':Destroy()
                        end
                    end
                    end
                    end
                end
            elseif TabName == 'Hat' then
                if Thing == 'Give Hat' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        local INS = game:GetService("InsertService")
                        local Q = INS:LoadAsset(Val):GetChildren()[1]
                        Q.Parent = v.Character
                    end
                    end
                    end
                end
            elseif TabName == 'Gear' then
                if Thing == 'Give Gear' then
                    local Players = GetPlayers()
                    for i,c in pairs(Players) do
                    for i,v in pairs(game:GetService("Players"):GetPlayers())do
                    if v.Name == c then
                        local INS = game:GetService("InsertService")
                        local Q = INS:LoadAsset(Val):GetChildren()[1]
                        Q.Parent = v:FindFirstChildWhichIsA("Backpack")
                    end
                    end
                    end
                end
            elseif TabName == 'Music' then
                if Thing == 'Play' then
                    for i,v in pairs(game:GetService('JointsService'):GetChildren())do
                        if v.Name == 'RxMusic' then
                            v:Destroy()
                        end
                    end
                    local SoundID = Val[1]
                    local Vol = Val[2]
                    local Pitch = Val[3]
                    if Vol == "" then
                        Vol = 1
                    elseif Pitch == "" then
                        Pitch = 1
                    end
                    local NewSound = Instance.new("Sound",game:GetService('JointsService'))
                    NewSound.SoundId = 'rbxassetid://'..SoundID
                    NewSound.Looped = true
                    NewSound.Pitch = Pitch
                    NewSound.Volume = Vol
                    NewSound:Play()
                    NewSound.Name = "RxMusic"
                elseif Thing == 'Stop' then
                    for i,v in pairs(game:GetService('JointsService'):GetChildren())do
                        if v.Name == 'RxMusic' then
                            v:Destroy()
                        end
                    end
                end
            elseif TabName == 'Options' then
                if Thing == 'AntiSkid' then
                    AntiSkid = not AntiSkid
                elseif Thing == 'Anti Chat (Anti ban)' then
                    AntiChat = not AntiChat
                    if AntiChat == true then
                        DoAnti()
                    else
                        RemoveAnti()
                    end
                elseif Thing == 'SpawnR6' then
                    SpawnR6 = not SpawnR6
                end
            end
        end
    end
    coroutine.wrap(QRTLP_fake_script)()
    local function GKOCZF_fake_script() -- Chat_C.nil.LocalScript 
        local script = Instance.new('LocalScript', Chat_C.nil)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        game:GetService("RunService").RenderStepped:Connect(function()
            for i,v in pairs(workspace:GetChildren())do
                if v:FindFirstChild"Leg" then
                    v:Destroy()
                end
            end
        end)
    end
    coroutine.wrap(GKOCZF_fake_script)()
    -- Chat_C.ToClientScripts.Lag is disabled.
    -- Chat_C.Chat_C.TheChat is disabled.
    -- Chat_C.nil.Credits is disabled.
    local function YUOJ_fake_script() -- Chat_C.Icon_2.LocalScript 
        local script = Instance.new('LocalScript', Chat_C.Icon_2)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local RANDOMNO = 3 --math.random(1,30)
        
        local Wait = 1.3
        
        local function Tween(number)
            local TS = game:GetService("TweenService")
            local A = TweenInfo.new(
                Wait,
                Enum.EasingStyle.Sine,
                Enum.EasingDirection.InOut,
                0,
                false,
                0
            )
            local B = {
                Rotation = number
            }
            TS:Create(script.Parent,A,B):Play()
        end
        local t = false
        while wait((Wait)+(.001)) do
            if t == true then
                t = false
                Tween(20)
            elseif t == false then
                t = true
                Tween(-20)
            end
        end
    end
    coroutine.wrap(YUOJ_fake_script)()
    local function NNDC_fake_script() -- Chat_C.TextLabel_8.TextAppear 
        local script = Instance.new('LocalScript', Chat_C.TextLabel_8)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local IntroText = "Ro-Xploit"
        local CredText = "By, Amokah"
        local is = script.Parent.Parent.INTRO
        local C = script.click
        script.Parent.Text = ""
        function AppearText(t,waitt)
            for i = 1,script.Parent.Text:len() do
                wait(.03)
                C.Pitch = C.Pitch - 0.05
                C:Play()
                script.Parent.Text = script.Parent.Text:sub(0,script.Parent.Text:len()-1)
            end
            for i = 1,#t do
                wait(waitt)
                C.Pitch = 1
                C:Play()
                script.Parent.Text = t:sub(1,i)
            end
        end
        
        spawn(function()
        repeat wait() until is.IsLoaded == true
        is:Play()
        game:GetService("TweenService"):Create(is,TweenInfo.new(5),{Volume = 1}):Play()
        end)
        
        AppearText(IntroText,.1)
        wait(1)
        AppearText(CredText,.1)--.05*1.2)
        wait(1)
        AppearText()
    end
    coroutine.wrap(NNDC_fake_script)()
    local function TLQAVC_fake_script() -- Chat_C.i.TextAppear 
        local script = Instance.new('LocalScript', Chat_C.i)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
            local Blur = Instance.new("BlurEffect",game:GetService("Lighting"))
            Blur.Size = 0
            game:GetService("TweenService"):Create(Blur,TweenInfo.new(.5),{Size = 50}):Play()
        local IntroText = "Ro-Xploit"
        local CredText = "By, Amokah"
        local C = script.click
        script.Parent.Text = ""
        function AppearText(t,waitt)
            for i = 1,script.Parent.Text:len() do
                wait(.03)
                C.Pitch = C.Pitch - 0.05
                C:Play()
                script.Parent.Text = script.Parent.Text:sub(0,script.Parent.Text:len()-1)
            end
            for i = 1,#t do
                wait(waitt)
                C.Pitch = 1
                C:Play()
                script.Parent.Text = t:sub(1,i)
            end
        end
        
        AppearText("Preloading GUI assets, please wait.",.15)
        
        spawn(function()
            repeat wait() until script.Parent.Parent.Parent:FindFirstChild'Main' ~= nil
            warn('GotMain')
            game:GetService("ContentProvider"):PreloadAsync({script:FindFirstAncestorWhichIsA("ScreenGui")})
            AppearText("Waiting a few more seconds so you enjoy this lovely music :)",.1)
            wait(math.random(1,5))
            game:GetService("TweenService"):Create(script.Parent.Parent.INTRO,TweenInfo.new(2),{Volume = 0}):Play()
            wait(2)
            script.Parent.Parent.Icon.LocalScript.Disabled = true
            game:GetService("TweenService"):Create(script.Parent.Parent.Icon,TweenInfo.new(.5),{Rotation = 0}):Play()
            wait(.5)
            game:GetService("TweenService"):Create(script.Parent.Parent.Icon,TweenInfo.new(2,Enum.EasingStyle.Sine),{Size = UDim2.new(0,0,0,0)}):Play()
            game:GetService("TweenService"):Create(script.Parent.Parent.Icon,TweenInfo.new(2),{Position = UDim2.new(0.5,0,0.5,0)}):Play()
            game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(2,Enum.EasingStyle.Back),{TextStrokeTransparency = 1}):Play()
            game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(2,Enum.EasingStyle.Back),{TextTransparency = 1}):Play()
            game:GetService("TweenService"):Create(Blur,TweenInfo.new(2),{Size = 0}):Play()
            game:GetService("Debris"):AddItem(Blur,2.5)
            wait(2)
            script.Parent.Parent.Parent.Button.Visible = true
        end)
    end
    coroutine.wrap(TLQAVC_fake_script)()
    local function HDLF_fake_script() -- Chat_C.Button_2.LocalScript 
        local script = Instance.new('LocalScript', Chat_C.Button_2)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local open = script.Parent.OpenButton
        local Main = script:FindFirstAncestorWhichIsA("ScreenGui"):WaitForChild'Main'
        local MainPosition = Main.Position
        local OutofSide = Main.Position + UDim2.new(0,0,-5,0)
        Main.Position = OutofSide
        local OpenNotVis = script.Parent.Position + UDim2.new(-5,0,0,0)
        Main.Visible = true
        game:GetService("RunService").Stepped:Wait()
        Main.Visible = false
        open.MouseButton1Click:Connect(function()
            Main.Visible = true
            game:GetService("TweenService"):Create(Main,TweenInfo.new(.3,Enum.EasingStyle.Sine),{Position = MainPosition}):Play()
            Main.Sounds.PopupSound:Play()
            InvisAllButtons()
        end)
        Main.TitleBar.Frame.Close.MouseButton1Click:Connect(function()
            game:GetService("TweenService"):Create(Main,TweenInfo.new(.5,Enum.EasingStyle.Sine),{Position = OutofSide}):Play()
            Main.Sounds.ClosingSound:Play()
            ShowAllButtons()
        end)
        
        function InvisAllButtons()
            game:GetService("TweenService"):Create(script.Parent.TextLabel,TweenInfo.new(.3,Enum.EasingStyle.Sine),{TextStrokeTransparency = 1}):Play()
            game:GetService("TweenService"):Create(script.Parent.TextLabel,TweenInfo.new(.3,Enum.EasingStyle.Sine),{TextTransparency = 1}):Play()
            game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(.3,Enum.EasingStyle.Sine),{ImageTransparency = 1}):Play()
            script.Parent.OpenButton.Visible = false
        end
        function ShowAllButtons()
            game:GetService("TweenService"):Create(script.Parent.TextLabel,TweenInfo.new(.3,Enum.EasingStyle.Sine),{TextStrokeTransparency = 0.3}):Play()
            game:GetService("TweenService"):Create(script.Parent.TextLabel,TweenInfo.new(.3,Enum.EasingStyle.Sine),{TextTransparency = 0}):Play()
            game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(.3,Enum.EasingStyle.Sine),{ImageTransparency = 0}):Play()
            script.Parent.OpenButton.Visible = true
        end
        
        ---------------------------------------
        
        
        local RANDOMNO = 3 --math.random(1,30)
        
        local Wait = 3
        
        local function Tween(number)
            local TS = game:GetService("TweenService")
            local A = TweenInfo.new(
                Wait,
                Enum.EasingStyle.Sine,
                Enum.EasingDirection.InOut,
                0,
                false,
                0
            )
            local B = {
                Rotation = number
            }
            TS:Create(script.Parent,A,B):Play()
        end
        local t = false
        if t == true then
                t = false
                Tween(20)
            elseif t == false then
                t = true
                Tween(-20)
            end
        while wait((Wait)+(.001)) do
            if t == true then
                t = false
                Tween(20)
            elseif t == false then
                t = true
                Tween(-20)
            end
        end
    end
    coroutine.wrap(HDLF_fake_script)()
