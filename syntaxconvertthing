        end
        
        function hecvode()
            chatfunc("Just disappear.",MAINRUINCOLOR.Color,"Inverted","Arcade",5)
            attack = true
            hum.WalkSpeed = 0
            CFuncs["Sound"].Create("rbxassetid://1368583274", root, 8, 0.5)
            CFuncs["Sound"].Create("rbxassetid://1368583274", root, 10, 0.7)
            CFuncs["Sound"].Create("rbxassetid://1368598393", root, 10, 0.4)
            CFuncs["Sound"].Create("rbxassetid://1368598393", root, 10, 1.2)
            for i = 0, 5, 0.02 do
            swait()
            slash(math.random(50,100)/10,5,true,"Round","Add","Out",root.CFrame*CFrame.new(0,-3,0)*CFrame.Angles(math.rad(math.random(-5,5)),math.rad(math.random(-360,360)),math.rad(math.random(-5,5))),vt(0.05,0.01,0.05),math.random(25,50)/250*i,BrickColor.new("Really black"))
            sphere2(5,"Add",larm.CFrame*CFrame.new(0,-1.5,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(1*i,1,1*i),-0.01*i,0.1*i,-0.01*i,BrickColor.new("Black"),BrickColor.new("Black").Color)
            slash(math.random(20,40)/10,5,true,"Round","Add","Out",larm.CFrame*CFrame.new(0,-1.5,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.025,0.001,0.025),-0.025*i,BrickColor.new("Really black"))
            RH.C0=clerp(RH.C0,cf(1,-1,0)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-6),math.rad(0),math.rad(-6)),.3)
            LH.C0=clerp(LH.C0,cf(-1,-1,0)*angles(math.rad(0),math.rad(-90),math.rad(0))*angles(math.rad(-3),math.rad(30),math.rad(3)),.3)
            RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(-50)),.3)
            Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(-15),math.rad(5),math.rad(50)),.3)
            RW.C0=clerp(RW.C0,cf(1.45,0.5,0.1)*angles(math.rad(-13),math.rad(-40),math.rad(20)),.3)
            LW.C0=clerp(LW.C0,cf(-1.45,0.5,0.1)*angles(math.rad(170),math.rad(10),math.rad(0)),.3)
            end
            CFuncs["Sound"].Create("rbxassetid://3417131625", char, 5, 1.2)
            for i = 0, 2, 0.1 do
            swait()
            slash(math.random(50,100)/10,5,true,"Round","Add","Out",root.CFrame*CFrame.new(0,-3,0)*CFrame.Angles(math.rad(math.random(-5,5)),math.rad(math.random(-360,360)),math.rad(math.random(-5,5))),vt(0.05,0.01,0.05),math.random(25,50)/50,BrickColor.new("Really black"))
            sphere2(5,"Add",larm.CFrame*CFrame.new(0,-1.5,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(5,1,5),-0.05,0.5,-0.05,BrickColor.new("Black"),BrickColor.new("Black").Color)
            slash(math.random(20,40)/10,5,true,"Round","Add","Out",larm.CFrame*CFrame.new(0,-1.5,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.025,0.001,0.025),-0.125,BrickColor.new("Really black"))
            RH.C0=clerp(RH.C0,cf(1,-1,0)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-6),math.rad(0),math.rad(-6)),.3)
            LH.C0=clerp(LH.C0,cf(-1,-1,0)*angles(math.rad(0),math.rad(-90),math.rad(0))*angles(math.rad(-3),math.rad(40),math.rad(3)),.3)
            RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(-90)),.3)
            Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(5),math.rad(0),math.rad(90)),.3)
            RW.C0=clerp(RW.C0,cf(1.45,0.5,0.1)*angles(math.rad(-13),math.rad(-20),math.rad(20)),.3)
            LW.C0=clerp(LW.C0,cf(-1.25,0.5,-0.5)*angles(math.rad(100),math.rad(0),math.rad(60)),.3)
            end
            CFuncs["Sound"].Create("rbxassetid://1664711478", char, 2, 1)
            CFuncs["Sound"].Create("rbxassetid://1177785010", char, 3, 1)
            CFuncs["Sound"].Create("rbxassetid://167115397", char, 3, 1)
            CFuncs["Sound"].Create("rbxassetid://782353443", char, 3, 0.9)
            CFuncs["Sound"].Create("rbxassetid://782353443", char, 4, 0.8)
            CFuncs["Sound"].Create("rbxassetid://782353443", char, 5, 0.7)
            for i = 0, 49 do
            slash(math.random(10,50)/10,5,true,"Round","Add","Out",root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.25,0.01,0.25),math.random(150,1000)/250,BrickColor.new("Really black"))
            end
            coroutine.resume(coroutine.create(function()
            local eff = Instance.new("ParticleEmitter",root)
