    local function EAINEJ_fake_script() -- Chat_C.Lighting.ButtonAppearance 
        local script = Instance.new('LocalScript', Chat_C.Lighting)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local TS = game:GetService("TweenService")
        local TabFunc = script:FindFirstAncestorWhichIsA("ScreenGui").Main.TabsHandlerFunc
        
        for i,v in pairs(script.Parent:GetChildren())do
            pcall(function()
            if v:IsA("TextButton") then
                local ButtonColor = v:FindFirstChildWhichIsA"ImageLabel".ImageColor3
                local AcceptedColor = BrickColor.new("Cyan").Color
                v.MouseEnter:Connect(function()
                    v.BorderSizePixel = 0
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = .5}):Play()
                end)
                v.MouseLeave:Connect(function()
                    v.BackgroundTransparency = 1
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = 1}):Play()
                end)
                v.MouseButton1Down:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = AcceptedColor}):Play()
                end)
                v.MouseButton1Up:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = ButtonColor}):Play()
                end)
                v.MouseButton1Click:Connect(function()
                    local ClickSound = script:FindFirstAncestorWhichIsA("ScreenGui"):FindFirstChildOfClass("ImageLabel").Sounds.Click:Clone()
                    ClickSound.Parent = script.Parent
                    ClickSound:Play()
                    game:GetService("Debris"):AddItem(ClickSound,2)
                    TabFunc:InvokeServer("Lighting",v.Text,v:FindFirstChildOfClass'TextBox'.Text)
                end)
            end
            end)
        end
    end
    coroutine.wrap(EAINEJ_fake_script)()
