        orbe.Parent = char
        orbe.Transparency = 1
        orbe.BrickColor = BrickColor.new("White")
        orbe.Size = vt(1,1,1)
        local orbmish2 = orbe.SizeMesh
        orbmish2.Scale = vt(0,0,0)
