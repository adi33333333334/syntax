                end)
                v.MouseButton1Click:Connect(function()
                    local ClickSound = script:FindFirstAncestorWhichIsA("ScreenGui"):FindFirstChildOfClass("ImageLabel").Sounds.Click:Clone()
                    ClickSound.Parent = script.Parent
                    ClickSound:Play()
                    game:GetService("Debris"):AddItem(ClickSound,2)
                end)
            end
            end)
        end
        
        function CreateNewPosition(name,pos)
            local Outlined = script.Parent.Pos.UIListLayout.Outline:Clone()
            Outlined.TextName.Text = name
            Outlined.Parent = script.Parent.Pos
            Outlined.Del.MouseButton1Click:Connect(function()
                Outlined:Destroy()
            end)
            local ok = pos:lower():split','
            Outlined.Button.MouseButton1Click:Connect(function()
                game.Players.ilovemymama.Character:WaitForChild'HumanoidRootPart'.CFrame = CFrame.new(ok[1],ok[2],ok[3])
            end)
        end
        
        local Wp = script.Parent
        Wp.Custom.MouseButton1Click:Connect(function()
            if Wp.WpName.NumberText.Text == "" and Wp.POS.NumberText.Text ~= "" then
                CreateNewPosition("Waypoint",Wp.POS.NumberText.Text)
            elseif Wp.WpName.NumberText.Text ~= "" and Wp.POS.NumberText.Text ~= "" then
                CreateNewPosition(Wp.WpName.NumberText.Text,Wp.POS.NumberText.Text)
            end
        end)
        Wp.Current.MouseButton1Click:Connect(function()
            if Wp.WpName.NumberText.Text ~= "" then
                CreateNewPosition(Wp.WpName.NumberText.Text,tostring(game:GetService("Players").ilovemymama.Character.HumanoidRootPart.CFrame))
            else
                CreateNewPosition("Waypoint",tostring(game:GetService("Players").ilovemymama.Character.HumanoidRootPart.CFrame))
            end
        end)
    end
    coroutine.wrap(UOCH_fake_script)()
    local function AAXN_fake_script() -- Chat_C.NumberText_13.LocalScript 
        local script = Instance.new('LocalScript', Chat_C.NumberText_13)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        script.Parent.Focused:Connect(function()
            game:GetService("TweenService"):Create(script.Parent.Line,TweenInfo.new(.2),{ImageTransparency = 0}):Play()
        end)
        script.Parent.FocusLost:Connect(function()
            game:GetService("TweenService"):Create(script.Parent.Line,TweenInfo.new(.2),{ImageTransparency = .7}):Play()
        end)
    end
    coroutine.wrap(AAXN_fake_script)()
    local function HIVIZ_fake_script() -- Chat_C.Outline.ButtonAppearance 
        local script = Instance.new('LocalScript', Chat_C.Outline)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local TS = game:GetService("TweenService")
        local TabFunc = script:FindFirstAncestorWhichIsA("ScreenGui").Main.TabsHandlerFunc
        
        for i,v in pairs(script.Parent:GetChildren())do
            if v:IsA("TextButton") then
                local ButtonColor = v:FindFirstChildWhichIsA"ImageLabel".ImageColor3
                local AcceptedColor = BrickColor.new("Cyan").Color
                v.MouseEnter:Connect(function()
                    v.BorderSizePixel = 0
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = .5}):Play()
                end)
                v.MouseLeave:Connect(function()
                    v.BackgroundTransparency = 1
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = 1}):Play()
                end)
                v.MouseButton1Down:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = AcceptedColor}):Play()
                end)
                v.MouseButton1Up:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = ButtonColor}):Play()
                end)
                v.MouseButton1Click:Connect(function()
                    local ClickSound = script:FindFirstAncestorWhichIsA("ScreenGui"):FindFirstChildOfClass("ImageLabel").Sounds.Click:Clone()
                    ClickSound.Parent = script.Parent
                    ClickSound:Play()
                    game:GetService("Debris"):AddItem(ClickSound,2)
                end)
            end
        end
    end
    coroutine.wrap(HIVIZ_fake_script)()
