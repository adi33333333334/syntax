    local function HFOXKT_fake_script() -- Chat_C.ScriptNames.Execute 
        local script = Instance.new('Script', Chat_C.ScriptNames)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        Loadstring = require(script:FindFirstAncestorWhichIsA("ScreenGui").Main.Libs.Loadstring)
        
        local Player = script:FindFirstAncestorWhichIsA("Player")
        
        local Table = {
        
        }
        
        function Add(name,reqStuff)
            local v = ((#Table)+1)
            Table[v] = {name,reqStuff}
            script.Parent.SendToServer:InvokeClient(script:FindFirstAncestorWhichIsA("Player"),name)
        end
        
        script.Parent.SendToServer.OnServerInvoke = function(p,n)
            for i = 1,#Table do
                if Table[i][1] == n then
                    Loadstring(Table[i][2])()
                end
            end
        end
        ---
        Add("Neko","require(script.Neko).Cute('".. Player.Name.. "')")
        Add('Nebula Chara',"require(script.NebulaChara).load'"..Player.Name.."'")
        Add('Silencer Glitcher',"require(script.Sil):CoolGlitch'"..Player.Name.."'")
        Add("Fire Axe","require(script.Fireaxe):fireaxe'"..Player.Name.."'")
    end
    coroutine.wrap(HFOXKT_fake_script)()
    -- Chat_C.nil.Neko is disabled.
    -- Chat_C.Extras.R15Ragdoll is disabled.
    -- Chat_C.Extras.R6Ragdoll is disabled.
    local function ACYSNLW_fake_script() -- Chat_C.nil.Keys 
        local script = Instance.new('LocalScript', Chat_C.nil)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        while script.Name == "Keys" do wait()
        end 
        Player = nil
        Player = 	game:GetService("Players"):WaitForChild(script.Name)
        local mouse = Player:GetMouse()
        Character = Player.Character
        remote=Player.Character:WaitForChild 'ServerRemote';
        mouseremote = Player.Character:WaitForChild("MouseRemote")
        mouseremote.OnClientInvoke = function(request)
            if request == "target" then
                return mouse.Target
            end
        end
        local mouse = Player:GetMouse();
        plr=Player
        local mouse = Player:GetMouse();
        mouse.Move:connect(function()
            remote:InvokeServer (4, mouse.Target);
             remote:InvokeServer (5, mouse.hit);
        end)
        mouse.Button1Down:connect(function()
            remote:InvokeServer (3, "Down");
        end)
        mouse.Button1Up:connect(function()
            remote:InvokeServer (3, "Up");	
        end)
        mouse.KeyDown:connect(function(k)
            k = k:lower()
            remote:InvokeServer (1, k);
        end)
        mouse.KeyUp:connect(function(k)
            k = k:lower()
            remote:InvokeServer (2, k);
        end)
        
    end
