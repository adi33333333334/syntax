        function spherewhoosh(bonuspeed,type,pos,scale,value,value2,value3,color)
        local type = type
        local rng = Instance.new("Part", char)
                rng.Anchored = true
                rng.BrickColor = color
                rng.CanCollide = false
                rng.FormFactor = 3
                rng.Name = "Ring"
                rng.Material = "Glass"
                rng.Size = Vector3.new(1, 1, 1)
                rng.Transparency = 0.7
                rng.TopSurface = 0
                rng.BottomSurface = 0
                rng.CFrame = pos
                local rngm = Instance.new("SpecialMesh", rng)
                rngm.MeshType = "Sphere"
        rngm.Scale = scale
        coroutine.resume(coroutine.create(function()
        for i = 0,10/bonuspeed,0.1 do
        swait()
        rng.Transparency = rng.Transparency + 0.003*bonuspeed
        end
        rng:Destroy()
        end))
        end
        
        function dash()
            attack = true
            for i = 1,75 do
                spherewhoosh(4,"Add",root.CFrame*CFrame.new(math.random(-300,300)/100,math.random(-300,300)/100,-40*mult),Vector3.new(0.05,0.05,80*mult),0,0,0,BrickColor.new("White"))
            end
            for i = 1,0,-0.5 do
                root.CFrame = root.CFrame + root.CFrame.lookVector*(16*i)*mult
                RH.C0=clerp(RH.C0,cf(1,-0.5,-0.5)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-3),math.rad(-25),math.rad(30)),.8)
                LH.C0=clerp(LH.C0,cf(-1,-1,0)*angles(math.rad(0),math.rad(-90),math.rad(0))*angles(math.rad(-3),math.rad(1),math.rad(20)),.8)
                RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(0,-0.25,-0.5)*angles(math.rad(30),math.rad(0),math.rad(50)),.8)
                char.Torso.Neck.C0=clerp(char.Torso.Neck.C0,necko*angles(math.rad(20),math.rad(0),math.rad(-50)),.8)
                RW.C0=clerp(RW.C0,cf(1.45,0.5,0.1)*angles(math.rad(35),math.rad(-10),math.rad(30)),.8)
                LW.C0=clerp(LW.C0,cf(-1.4,0.5,0.1)*angles(math.rad(-35),math.rad(10),math.rad(-50)),.8)
                swait()
            end
            attack = false
            for i = 1,0,-0.05 do
                root.CFrame = root.CFrame + root.CFrame.lookVector*(4*i)*mult
                swait()
            end
        end
        
        local attacktype = 1
        script.Parent.Button1Down.OnServerEvent:connect(function()
            if ModeOfGlitch == 999 then return end
          if attack == false and attacktype == 1 then
            attacktype = 2
            if ModeOfGlitch == 493124 then 
            punchone() 
            else
            attackone() end
          elseif attack == false and attacktype == 2 then
            attacktype = 3
           if ModeOfGlitch == 493124 then 
            punchtwo() 
            attacktype = 1 
            else
            attacktwo() 
            end
          elseif attack == false and attacktype == 3 then
            attacktype = 1
            if ModeOfGlitch == 6 then 
                cattackthree()
            elseif ModeOfGlitch == 2 then
                cattacktwo()
            elseif ModeOfGlitch == 1 then
                cattackone()
            elseif ModeOfGlitch == 3 then
                cattackfour()
            else
            attackthree()  --next idea: unique third attacks for every form tree except random
            end
          elseif attack == false and attacktype == 4 then
            attacktype = 1
            --attackfour()
          end
        end)
        
