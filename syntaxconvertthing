        function entrance()
                hum.WalkSpeed = 0
            if cooldowns then attack = true end
            local elocacenter = CreateParta(root,1,1,"SmoothPlastic",BrickColor.random())
            elocacenter.Anchored = true
            elocacenter.CFrame = root.CFrame
            local eloca1 = CreateParta(elocacenter,1,1,"SmoothPlastic",BrickColor.new("Really red"))
