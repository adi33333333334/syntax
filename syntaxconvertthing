                eff.Rotation = NumberRange.new(-500,500)
                eff.SpreadAngle = Vector2.new(0,900)
                eff.RotSpeed = NumberRange.new(-500,500)
                local eff2 = eff:Clone()
                eff2.Parent = hitb
                eff2.Lifetime = NumberRange.new(2)
                eff2.Speed = NumberRange.new(50*m) 
                eff2.Color = ColorSequence.new(BrickColor.new("White").Color)
                coroutine.resume(coroutine.create(function()
                swait(12)
                eff.Enabled = false
                eff2.Enabled = false
                    wait(3)
                    hitb:Destroy()
                end))
                --sphere((m+5)/5,"Add",hitb.CFrame,vt(40*m,40*m,40*m),-m/5,BrickColor.new("White"))
                MagniDamage(hitb, 20*m, 999999,9999999, 0, "UpKnock")
                m = m*multe*mult
                multe = multe - 0.05*mult
                swait()
            end
            end))
                for i = 0,number,0.1 do
                    swait()
                        RootJoint.C0 = clerp(RootJoint.C0,RootCF*cf(0,0,0)* angles(math.rad(-5),math.rad(0),math.rad(70)),0.4)
                        Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(20),math.rad(0),math.rad(-70)),.4)
