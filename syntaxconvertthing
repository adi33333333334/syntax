        lookv = lookv + 10
        rem.CFrame = rem.CFrame*CFrame.Angles(0,math.rad(rot),0)
        rem2.CFrame = rem.CFrame*CFrame.Angles(0,math.rad(90),0)
        rem3.CFrame = rem.CFrame*CFrame.Angles(0,math.rad(180),0)
        rem4.CFrame = rem.CFrame*CFrame.Angles(0,math.rad(270),0)
        orb_spawn_norm(rem.CFrame + rem.CFrame.lookVector*lookv,3,MAINRUINCOLOR,power,15,35,10,power/5,15)
        orb_spawn_norm(rem2.CFrame + rem2.CFrame.lookVector*lookv,3,MAINRUINCOLOR,power,15,35,10,power/5,15)
        orb_spawn_norm(rem3.CFrame + rem3.CFrame.lookVector*lookv,3,MAINRUINCOLOR,power,15,35,10,power/5,15)
        orb_spawn_norm(rem4.CFrame + rem4.CFrame.lookVector*lookv,3,MAINRUINCOLOR,power,15,35,10,power/5,15)
        end
        end))
        attack = false
        end
