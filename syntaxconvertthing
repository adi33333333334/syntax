        function ExtinctiveHeartbreak()
        local targetted = nil
        if script.Parent.mousetarg.Value.Parent ~= Character and script.Parent.mousetarg.Value.Parent.Parent ~= Character and script.Parent.mousetarg.Value.Parent:FindFirstChildOfClass("Humanoid") ~= nil then
        targetted = script.Parent.mousetarg.Value.Parent
        end
        if targetted ~= nil and (script.Parent.mousetarg.Value.Parent:FindFirstChildOfClass("Humanoid").Health < script.Parent.mousetarg.Value.Parent:FindFirstChildOfClass("Humanoid").MaxHealth/1.5 or instakill) then
        if cooldowns then attack = true end
