        block(2,"Add",root.CFrame,vt(1,1,1),6*2,6*2,6*2,MAINRUINCOLOR,MAINRUINCOLOR.Color)
        block(1,"Add",root.CFrame,vt(1,1,1),4.5*2,4.5*2,4.5*2,MAINRUINCOLOR,MAINRUINCOLOR.Color)
        end
        for i = 0, 49 do
