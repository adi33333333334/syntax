        locat.Transparency = 1
        locat.TopSurface = 0
        locat.BottomSurface = 0
        locat.Anchored = true
        locat.CFrame = root.CFrame*CFrame.new(0,-3,0)
        local poste = 0
        local rotation = 0
        local upperpos = 0
        local rate = 0
        local x = locat
        
        for i, v in pairs(mw2:GetChildren()) do
        if v:IsA("Part") then
        v.BrickColor = one
        v.Material = "Neon"
        end
        end
        
