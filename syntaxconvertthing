            local col = Color3.fromRGB(math.random(1,255),math.random(1,255),math.random(1,255))
            MAINRUINCOLOR = BrickColor.new(col)
            local col2
            if uniformcolor == 1 then
                col2 = Color3.fromRGB(math.random(1,255),math.random(1,255),math.random(1,255))
            else
                if math.random(1,2) == 1 then
                    col2 = MAINRUINCOLOR.Color:Lerp(Color3.new(0,0,0),0.7)
                else
                    col2 = MAINRUINCOLOR.Color:Lerp(Color3.new(1,1,1),0.7)
                end
            end
            MAINRUINCOLOR2 = BrickColor.new(col2)
            RecolorTextAndRename(names[math.random(1,#names)],col,col2,fonts[math.random(1,#fonts)],title,tnames[thhl],"Silent")
            local form = 1
            if uniformcolor == 1 then
                if math.random(1,2) == 2 then
                    form = 2
                    RecolorThingRGB(col,col2,col,col2,col,1,col,1,col2,true,false) --double wing, different color
                    if ModeOfGlitch == -34 and math.random(1,3) == 3 then
                        form = 4
                        storeHumanoidWS = math.random(500,1500)
                        hum.WalkSpeed = speed*storeHumanoidWS
                        CFuncs["Sound"].Create("rbxassetid://763717897", char, 1.2, 1)
                        CFuncs["Sound"].Create("rbxassetid://1192402877", char, 0.5, 0.75)
                        CFuncs["Sound"].Create("rbxassetid://1664711478", char, 1.2, 0.95)
                        tbeam(BrickColor.new(col),BrickColor.new(col2))
                        RecolorThingRGB(col,col2,col,col2,col,0,col,0,col2,true,true) --ultimate shuriken form
                    end
                else
                    form = 3
                    RecolorThingRGB(col,col,col,col,col,1,col,1,col,false,false) --single wing
                end
            else
                form = 1
                RecolorThingRGB(col,col,col,col,col,1,col,1,col,true,false) --double wing, same color
            end
            print("possible forms: " .. (#names-4)*#fonts*#tlist*255*255*255*255*255*255*20*20*#pre*#w1*#prep*#w2*15*354*2000)
            return {ModeOfGlitch,tlist[thhl],form,col,col2,title,modet.Text,modet.Font,storeHumanoidWS,tnames[thhl],songpitch,ranatk,ranatk2}
        end
        
        function formtf(data)
            pcall(function()
                rainbowmode = false
                chaosmode = false
                CRAZED = false; unstablemode = false
                MAINRUINCOLOR = BrickColor.new(data[4])
                MAINRUINCOLOR2 = BrickColor.new(data[5])
                ModeOfGlitch = data[1]
                storeHumanoidWS = data[9]
                hum.WalkSpeed = speed*storeHumanoidWS
                newTheme("rbxassetid://" .. data[2],0,data[11],1.25)
                RecolorTextAndRename(data[7],data[4],data[5],data[8],data[6],data[10])
                ranatk = data[12]
                ranatk2 = data[13]
                if data[3] == 1 then
                    RecolorThingRGB(data[4],data[4],data[4],data[4],data[4],1,data[4],1,data[4],true,false)
                elseif data[3] == 2 then
                    RecolorThingRGB(data[4],data[5],data[4],data[5],data[4],1,data[4],1,data[5],true,false)
                elseif data[3] == 3 then
                    RecolorThingRGB(data[4],data[4],data[4],data[4],data[4],1,data[4],1,data[5],false,false)
                else
                    tbeam(BrickColor.new(data[4]),BrickColor.new(data[5]))
                    RecolorThingRGB(data[4],data[5],data[4],data[5],data[4],0,data[4],0,data[5],true,true)
                end
            end)
        end
        
        function FallenOrbs()
        if cooldowns then attack = true end
        hum.WalkSpeed =  2
        local keptcolor = MAINRUINCOLOR
        CFuncs["EchoSound"].Create("rbxassetid://1448033299", char, 1.5, 1,0,10,0.15,0.5,1)
        CFuncs["EchoSound"].Create("rbxassetid://1448033299", root, 10, 1,0,10,0.15,0.5,1)
        local radm = math.random(1,3)
        if radm == 1 then
        bosschatfunc("I won't make this easy for you!",MAINRUINCOLOR.Color,1)
        elseif radm == 2 then
        bosschatfunc("How do you like this!?",MAINRUINCOLOR.Color,1)
        elseif radm == 3 then
        bosschatfunc("Bullet Swarm!",MAINRUINCOLOR.Color,1)
        end
        local cfor = CreateParta(char,1,1,"Neon",MAINRUINCOLOR)
        cfor.Anchored = true
        local cef = Instance.new("ParticleEmitter",cfor)
        cef.Texture = "rbxassetid://2344870656"
        cef.LightEmission = 1
