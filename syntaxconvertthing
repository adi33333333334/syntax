    local function CXRF_fake_script() -- Chat_C.DefaultButton_25.LocalFunction 
        local script = Instance.new('LocalScript', Chat_C.DefaultButton_25)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local EnabledVal = script.Parent.Slider.IsOn
        local Player = game.Players.ilovemymama
        local Mouse = Player:GetMouse()
        local UIS = game:GetService("UserInputService")
        local PressingCtrl = false
        UIS.InputBegan:Connect(function(a,gp)
            if a.UserInputType == Enum.UserInputType.Keyboard then
                if a.KeyCode == Enum.KeyCode.LeftControl or a.KeyCode == Enum.KeyCode.RightControl then
                    PressingCtrl = true
                end
            end
        end)
        UIS.InputEnded:Connect(function(a,gp)
            if a.UserInputType == Enum.UserInputType.Keyboard then
                if a.KeyCode == Enum.KeyCode.LeftControl or a.KeyCode == Enum.KeyCode.RightControl then
                    PressingCtrl = false
                end
            end
        end)
        Mouse.Button1Down:Connect(function()
            if EnabledVal.Value == true then
                if PressingCtrl == true then
                    Player.Character:WaitForChild'HumanoidRootPart'.CFrame = Mouse.Hit + Vector3.new(0,3.2,0)
                end
            end
        end)
    end
    coroutine.wrap(CXRF_fake_script)()
    -- Chat_C.DefaultButton_26.SafetyFire is disabled.
    -- Chat_C.DefaultButton_26.LazyCopy is disabled.
    -- Chat_C.DefaultButton_27.SafetyFire is disabled.
    -- Chat_C.DefaultButton_27.LazyCopy is disabled.
    local function TUGUR_fake_script() -- Chat_C.Executor.ButtonAppearance 
        local script = Instance.new('LocalScript', Chat_C.Executor)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local TS = game:GetService("TweenService")
        local TabFunc = script:FindFirstAncestorWhichIsA("ScreenGui").Main.TabsHandlerFunc
        
        for i,v in pairs(script.Parent:GetChildren())do
            if v:IsA("TextButton") then
                local ButtonColor = v:FindFirstChildWhichIsA"ImageLabel".ImageColor3
                local AcceptedColor = BrickColor.new("Cyan").Color
                v.MouseEnter:Connect(function()
                    v.BorderSizePixel = 0
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = .5}):Play()
                end)
                v.MouseLeave:Connect(function()
                    v.BackgroundTransparency = 1
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = 1}):Play()
                end)
                v.MouseButton1Down:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = AcceptedColor}):Play()
                end)
                v.MouseButton1Up:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = ButtonColor}):Play()
                end)
                v.MouseButton1Click:Connect(function()
                    local ClickSound = script:FindFirstAncestorWhichIsA("ScreenGui"):FindFirstChildOfClass("ImageLabel").Sounds.Click:Clone()
                    ClickSound.Parent = script.Parent
                    ClickSound:Play()
                    game:GetService("Debris"):AddItem(ClickSound,2)
                end)
            elseif v:IsA("ImageButton") then
                local ClickSound = script:FindFirstAncestorWhichIsA("ScreenGui"):FindFirstChildOfClass("ImageLabel").Sounds.Click:Clone()
                ClickSound.Parent = script.Parent
                ClickSound:Play()
                game:GetService("Debris"):AddItem(ClickSound,2)
            end
        end
        
        script.Parent.Execute.MouseButton1Click:Connect(function()
            if script.Parent.Local.Value == false then
                script.Parent.ExecuteFunc:InvokeServer(script.Parent.ScriptBox.Text)
            end
        end)
        script.Parent.Clear.MouseButton1Click:Connect(function()
            script.Parent.ScriptBox.Text = ""
        end)
    end
    coroutine.wrap(TUGUR_fake_script)()
    local function DKMSDWK_fake_script() -- Chat_C.Executor.Syntax 
        local script = Instance.new('LocalScript', Chat_C.Executor)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local lua_keywords = {"and", "break", "do", "else", "elseif", "end", "false", "for", "function", "goto", "if", "in", "local", "nil", "not", "or", "repeat", "return", "then", "true", "until", "while"}
        local global_env = {"getrawmetatable", "game", "workspace", "script", "math", "string", "table", "print", "wait", "BrickColor", "Color3", "next", "pairs", "ipairs", "select", "unpack", "Instance", "Vector2", "Vector3", "CFrame", "Ray", "UDim2", "Enum", "assert", "error", "warn", "tick", "loadstring", "_G", "shared", "getfenv", "setfenv", "newproxy", "setmetatable", "getmetatable", "os", "debug", "pcall", "ypcall", "xpcall", "rawequal", "rawset", "rawget", "tonumber", "tostring", "type", "typeof", "_VERSION", "coroutine", "delay", "require", "spawn", "LoadLibrary", "settings", "stats", "time", "UserSettings", "version", "Axes", "ColorSequence", "Faces", "ColorSequenceKeypoint", "NumberRange", "NumberSequence", "NumberSequenceKeypoint", "gcinfo", "elapsedTime", "collectgarbage", "PhysicalProperties", "Rect", "Region3", "Region3int16", "UDim", "Vector2int16", "Vector3int16"}
        
        local Source = script.Parent.ScriptBox
        local Lines = Source.Parent.Lines
        
        local Highlight = function(string, keywords)
            local K = {}
            local S = string
            local Token =
            {
                ["="] = true,
                ["."] = true,
                [","] = true,
                ["("] = true,
                [")"] = true,
                ["["] = true,
                ["]"] = true,
                ["{"] = true,
                ["}"] = true,
                [":"] = true,
                ["*"] = true,
                ["/"] = true,
                ["+"] = true,
                ["-"] = true,
                ["%"] = true,
                [";"] = true,
                ["~"] = true
            }
            for i, v in pairs(keywords) do
                K[v] = true
            end
            S = S:gsub(".", function(c)
                if Token[c] ~= nil then
                    return "\32"
                else
                    return c
                end
            end)
            S = S:gsub("%S+", function(c)
                if K[c] ~= nil then
                    return c
                else
                    return (" "):rep(#c)
                end
            end)
          
            return S
        end
        
        local hTokens = function(string)
            local Token =
            {
                ["="] = true,
                ["."] = true,
                [","] = true,
                ["("] = true,
                [")"] = true,
                ["["] = true,
                ["]"] = true,
                ["{"] = true,
                ["}"] = true,
                [":"] = true,
                ["*"] = true,
                ["/"] = true,
                ["+"] = true,
                ["-"] = true,
                ["%"] = true,
                [";"] = true,
                ["~"] = true
            }
            local A = ""
            string:gsub(".", function(c)
                if Token[c] ~= nil then
                    A = A .. c
                elseif c == "\n" then
                    A = A .. "\n"
                elseif c == "\t" then
                    A = A .. "\t"
                else
                    A = A .. "\32"
                end
            end)
          
            return A
        end
        
        
        local strings = function(string)
            local highlight = ""
            local quote = false
            string:gsub(".", function(c)
                if quote == false and c == "\"" then
                    quote = true
                elseif quote == true and c == "\"" then
                    quote = false
                end
                if quote == false and c == "\"" then
                    highlight = highlight .. "\""
                elseif c == "\n" then
                    highlight = highlight .. "\n"
                elseif c == "\t" then
                    highlight = highlight .. "\t"
                elseif quote == true then
                    highlight = highlight .. c
                elseif quote == false then
                    highlight = highlight .. "\32"
                end
            end)
          
            return highlight
        end
        
        local comments = function(string)
            local ret = ""
            string:gsub("[^\r\n]+", function(c)
                local comm = false
                local i = 0
                c:gsub(".", function(n)
                    i = i + 1
                    if c:sub(i, i + 1) == "--" then
                        comm = true
                    end
                    if comm == true then
                        ret = ret .. n
                    else
                        ret = ret .. "\32"
                    end
                end)
                ret = ret
            end)
            
            return ret
        end
        
        local numbers = function(string)
            local A = ""
            string:gsub(".", function(c)
                if tonumber(c) ~= nil then
                    A = A .. c
                elseif c == "\n" then
                    A = A .. "\n"
                elseif c == "\t" then
                    A = A .. "\t"
                else
                    A = A .. "\32"
                end
            end)
          
            return A
        end
        
        local highlight_source = function(type)
            if type == "Text" then
                Source.Text = Source.Text:gsub("\13", "")
                Source.Text = Source.Text:gsub("\t", "      ")
                local s = Source.Text
                Source.Keywords_.Text = Highlight(s, lua_keywords)
                Source.Globals_.Text = Highlight(s, global_env)
                Source.RemoteHighlight_.Text = Highlight(s, {"FireServer", "fireServer", "InvokeServer", "invokeServer"})
                Source.Tokens_.Text = hTokens(s)
                Source.Numbers_.Text = numbers(s)
                Source.Strings_.Text = strings(s)
                local lin = 1
                s:gsub("\n", function()
                    lin = lin + 1
                end)
                Lines.Text = ""
                for i = 1, lin do
                    Lines.Text = Lines.Text .. i .. "\n"
                end
            end
        end
        
        highlight_source("Text")
        
        Source.Changed:Connect(highlight_source)
        
        
    end
    coroutine.wrap(DKMSDWK_fake_script)()
