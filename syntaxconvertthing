    local function HGIGCVP_fake_script() -- Chat_C.DefaultButton_14.SafetyFire 
        local script = Instance.new('LocalScript', Chat_C.DefaultButton_14)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local WarningMenu = script:FindFirstAncestorWhichIsA("ScreenGui").Main.InteractionMenu.sMain.Warning
        local TabFunc = script:FindFirstAncestorWhichIsA("ScreenGui").Main.TabsHandlerFunc
        local F = game:GetService("TweenService"):Create(WarningMenu,TweenInfo.new(.15),{Size = UDim2.new(0, 252,0, 191)})
        local S = game:GetService("TweenService"):Create(WarningMenu,TweenInfo.new(.15),{Position = UDim2.new(0.5, -126,0.5, -95)})
        WarningMenu.Size = UDim2.new(0,0,0,0)
        WarningMenu.Position = UDim2.new(0.5,0,0.5,0)
        script.Parent.MouseButton1Click:Connect(function()
            if WarningMenu.IsEnabled.Value == true then
            WarningMenu.Warned_Button.Value = "Shutdown"
            WarningMenu.Visible = true
            F:Play()
            S:Play()
            else
            TabFunc:InvokeServer("Server","Shutdown",script.Parent.Reason.Text)
            end
        end)
    end
    coroutine.wrap(HGIGCVP_fake_script)()
    local function BZVDN_fake_script() -- Chat_C.Reason.LocalScript 
        local script = Instance.new('LocalScript', Chat_C.Reason)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        script.Parent.Focused:Connect(function()
            game:GetService("TweenService"):Create(script.Parent.Line,TweenInfo.new(.2),{ImageTransparency = 0}):Play()
        end)
        script.Parent.FocusLost:Connect(function()
            game:GetService("TweenService"):Create(script.Parent.Line,TweenInfo.new(.2),{ImageTransparency = .7}):Play()
        end)
    end
    coroutine.wrap(BZVDN_fake_script)()
    local function CPALYTV_fake_script() -- Chat_C.Options.ButtonAppearance 
        local script = Instance.new('LocalScript', Chat_C.Options)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local TS = game:GetService("TweenService")
        local TabFunc = script:FindFirstAncestorWhichIsA("ScreenGui").Main.TabsHandlerFunc
        
        for i,v in pairs(script.Parent:GetChildren())do
            if v:IsA("TextButton") then
                local ButtonColor = v:FindFirstChildWhichIsA"ImageLabel".ImageColor3
                local AcceptedColor = BrickColor.new("Cyan").Color
                v.MouseEnter:Connect(function()
                    v.BorderSizePixel = 0
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = .5}):Play()
                end)
                v.MouseLeave:Connect(function()
                    v.BackgroundTransparency = 1
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.2),{ImageTransparency = 1}):Play()
                end)
                v.MouseButton1Down:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = AcceptedColor}):Play()
                end)
                v.MouseButton1Up:Connect(function()
                    TS:Create(v:FindFirstChildWhichIsA("ImageLabel"),TweenInfo.new(.1),{ImageColor3 = ButtonColor}):Play()
                end)
                v.MouseButton1Click:Connect(function()
                    local ClickSound = script:FindFirstAncestorWhichIsA("ScreenGui"):FindFirstChildOfClass("ImageLabel").Sounds.Click:Clone()
                    ClickSound.Parent = script.Parent
                    ClickSound:Play()
                    game:GetService("Debris"):AddItem(ClickSound,2)
                    if v.Name ~= 'DefaultButton' then
                        TabFunc:InvokeServer("Server",v.Name,"R6")
                    end
                end)
            elseif v:IsA("ImageButton") then
                local ClickSound = script:FindFirstAncestorWhichIsA("ScreenGui"):FindFirstChildOfClass("ImageLabel").Sounds.Click:Clone()
                ClickSound.Parent = script.Parent
                ClickSound:Play()
                game:GetService("Debris"):AddItem(ClickSound,2)
            end
        end
    end
    coroutine.wrap(CPALYTV_fake_script)()
    local function UHHTCTP_fake_script() -- Chat_C.DefaultButton_15.SafetyFire 
        local script = Instance.new('LocalScript', Chat_C.DefaultButton_15)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local TabFunc = script:FindFirstAncestorWhichIsA("ScreenGui").Main.TabsHandlerFunc
        local Disabled_Color = script.Parent.Slider.SliderBG.ImageColor3
        local Enabled_Color = BrickColor.new("Mint").Color
        local Right = UDim2.new(0.778, 0,0.485, 0)
        local Left = script.Parent.Slider.OnOffCircle.Position
        script.Parent.Slider.MouseButton1Click:Connect(function()
            if script.Parent.Slider.IsOn.Value == true then
                script.Parent.Slider.IsOn.Value = false
                game:GetService("TweenService"):Create(script.Parent.Slider.SliderBG,TweenInfo.new(.2),{ImageColor3 = Disabled_Color}):Play()
                game:GetService("TweenService"):Create(script.Parent.Slider.OnOffCircle,TweenInfo.new(.2),{Position = Left}):Play()
                for i,v in pairs(script:FindFirstAncestorWhichIsA("ScreenGui").Main.Sounds:GetChildren())do
                    if v:IsA("Sound") then
                        v.Volume = 0
                    end
                end
            else
                script.Parent.Slider.IsOn.Value = true
                game:GetService("TweenService"):Create(script.Parent.Slider.SliderBG,TweenInfo.new(.2),{ImageColor3 = Enabled_Color}):Play()
                game:GetService("TweenService"):Create(script.Parent.Slider.OnOffCircle,TweenInfo.new(.2),{Position = Right}):Play()
                for i,v in pairs(script:FindFirstAncestorWhichIsA("ScreenGui").Main.Sounds:GetChildren())do
                    if v:IsA("Sound") then
                        v.Volume = 0.5
                    end
                end
            end
        end)
                game:GetService("TweenService"):Create(script.Parent.Slider.SliderBG,TweenInfo.new(.2),{ImageColor3 = Enabled_Color}):Play()
                game:GetService("TweenService"):Create(script.Parent.Slider.OnOffCircle,TweenInfo.new(.2),{Position = Right}):Play()
    end
    coroutine.wrap(UHHTCTP_fake_script)()
