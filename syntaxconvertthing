        function swait(num)
            if (num == 0 or num == nil) then
                if speed then
                    if math.random(1,speed) ~= speed then return end
                end
                ArtificialHB.OnServerEvent:wait()
            else
                for i = 1, math.ceil(num/speed) do
                    ArtificialHB.OnServerEvent:wait()
                end
            end
        end
        
