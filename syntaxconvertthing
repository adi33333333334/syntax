        function randomform()
            local title = "A Random Form"
            local pre = {"A ","The ","One ","Supreme ","Greatest ","Lesser ","Divine ","Pure ","Violent ","Corrupt ","Chaotic ","Balanced ","Calamitous ",""}
            local w1 = {"Creator","Destroyer","Dominator","Denominator","Oven","Creation","Universe","Soul","Spirit","Form","Force","Eater","Fighter","Conqueror","Assassin","Culmination","Fusion","Destroyer","Partner","Friend","Rival","Guardian","Protector","Keeper","Reversal","Acceleration","Speed","Shock","Moai","Aura","Specter"}
            local prep = {" from "," of "," on "," in "," for "}
            local w2 = {"Desperation","Eternity","Time","Destruction","Space","the End","Darkness","Light","Infinity","Negativity","Positivity","Power","the World","the Universe","the Galaxy","Destiny","Caterpillars","Volcanism","the Unchangeable","Disease","Vaporwave","Sound","the Yosh","the Moai","the Goblins","the Strange","the Blurple","Legitimacy","Life","Death","Storms","Volcanism","Avalanches","Tornadoes","Hurricanes","Galaxies","the Galaxy","the Multiverse","Everything","Nothing","Something","Color","Brightness","Paint","Glue","Chocolate"}
            title = pre[math.random(1,#pre)] .. w1[math.random(1,#w1)] .. prep[math.random(1,#prep)] .. w2[math.random(1,#w2)]
            local uniformcolor = math.random(1,2)
            ModeOfGlitch = math.random(-48,-34)
            storeHumanoidWS = math.random(16,70)
            if ModeOfGlitch == -35 or ModeOfGlitch == -45 or ModeOfGlitch == -48 then
                storeHumanoidWS = math.random(100,200)
            elseif ModeOfGlitch == -34 then
                storeHumanoidWS = math.random(200,300)
            end
            ranatk = math.random(1,25)
            repeat
                ranatk2 = math.random(1,25)
            until ranatk2 ~= ranatk
            hum.WalkSpeed = speed*storeHumanoidWS
            rainbowmode = false
            chaosmode = false
            CRAZED = false; unstablemode = false
            local tlist = {577543579,2041343402,3279318421,1465004011,728272672,2024346699,625292881,257464674,318062185,402668289,2598224585,910143583,2475400182,318890513,2130908028,2485070323,2614150374,3422817411,228807963,2996307347,2204137310,2588085190,3165394887,3355655842,3401292474,606181609,1426899890,2569365263,2964842403,3255006996,3254997454,2128137966,1002071384,1751171913,1119709168,2127419486,604910909,3198589977,819373905,183142252,303345135,1248240943,2272275958,748325014,833779944,739066292,398455752,3070433217,3198592382,3221578654,1280010741,2071274388,2685427316,2666435584,2653360968,586436338,1276814239,1296208488,2038976967,1301290348,1566197356,2934442000,864793052,965845438,2862837317,3061585900,1464474710,1609011649,323468493,420320748,420320649,314921470,364750337,335759316,576075040,212693212,251954665,2504722412,982687613,1930926455,398200208,1290033819,2554670231,1853759713,3103496776,1228696343,1074484884,1588702539,1343241846,2507438099,1327427310,2025441769,2554668713,3103494654,1853501150,528065818,1788395627,2922592018,780257650,1327560508,1747459493,2127517667,1642843860,202697708}
