    -- Chat_C.nil.Shaker is disabled.
    -- Chat_C.nil.LocalScript is disabled.
    local function VEXVIP_fake_script() -- Chat_C.nil.dwairemover 
        local script = Instance.new('LocalScript', Chat_C.nil)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        script.Parent = game.Players.ilovemymama.PlayerGui
        local plr = game.Players.ilovemymama
        
        workspace.ChildRemoved:Connect(function(a)
            if a.Name == plr.Name then
                if plr.PlayerGui:FindFirstChild("dont worry about it") then
                    wait(.1)
                    plr.PlayerGui:FindFirstChild("dont worry about it"):Destroy()
                    script:Destroy()
                end
            end
        end)
    end
    coroutine.wrap(VEXVIP_fake_script)()
    local function YTZOHJE_fake_script() -- Chat_C.nil.FakeMouseMainConversion 
        local script = Instance.new('Script', Chat_C.nil)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        wait(1/60) 
        require(script:WaitForChild'EzConvert')()
        
        local S = setmetatable({},{__index = function(s,i) return game:service(i) end})
        local Plrs = S.Players
        local Plr = Plrs.LocalPlayer
        local Char = Plr.Character
        local Hum = Char:FindFirstChildOfClass'Humanoid'
