            local NewInstance = function(instance,parent,properties)
                local inst = Instance.new(instance,parent)
                if(properties)then
                    for i,v in next, properties do
                        pcall(function() inst[i] = v end)
                    end
                end
                return inst;
            end
            
        function bigpunch()
            Humanoid.WalkSpeed = 2
            local number = 3
            CFuncs["Sound"].Create("rbxassetid://838392947", root, 10, 0.8)
            CFuncs["Sound"].Create("rbxassetid://1368598393", root, 10, 0.8)
            if cooldowns then attack = true end
                for i = 0,number,0.1 do
                    slash(math.random(50,100)/10,5,true,"Round","Add","Out",root.CFrame*CFrame.new(0,-3,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.3,0.05,0.3),math.random(-100,-50)/250,BrickColor.new("White"))
                    swait()
                        RootJoint.C0 = clerp(RootJoint.C0,RootCF*cf(0,0,0)* angles(math.rad(20),math.rad(0),math.rad(-40)),0.2)
                        Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(0),math.rad(0),math.rad(40)),.2)
                         RW.C0 = clerp(RW.C0, CFrame.new(1.45, 0.5, 0) * angles(math.rad(90), math.rad(0), math.rad(-40)), 0.2)
                         LW.C0 = clerp(LW.C0, CFrame.new(-1.45, 0.5, 0) * angles(math.rad(90), math.rad(0), math.rad(-40)), 0.2)
            RH.C0=clerp(RH.C0,cf(1,-1 - 0.05 * math.cos(sine / 25),-0.75)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-5),math.rad(0),math.rad(20)),.2)
            LH.C0=clerp(LH.C0,cf(-1,-1 - 0.05 * math.cos(sine / 25),0)*angles(math.rad(0),math.rad(-90),math.rad(0))*angles(math.rad(-5),math.rad(0),math.rad(10)),.2)
                end
                CFuncs["EchoSound"].Create("rbxassetid://824687369", char, 10, 0.75,0,10,0.25,0.5,1)
                CFuncs["EchoSound"].Create("rbxassetid://824687369", char, 10, 1,0,10,0.25,0.5,1)
                CFuncs["EchoSound"].Create("rbxassetid://824687369", char, 10, 1.25,0,10,0.25,0.5,1)
                CFuncs["Sound"].Create("rbxassetid://1841058541", char, 10,1)
                CFuncs["Sound"].Create("rbxassetid://438666141", char, 10,0.6)
                CFuncs["Sound"].Create("rbxassetid://1208650519", char, 10, 0.6)
                CameraEnshaking(3,30)
                local multe = 2
                local m = 1
            coroutine.resume(coroutine.create(function()
                MagniDamage(root, 50, 9999999,99999999, 0, "Normal")
            for i = 1,50 do
                slash(math.random(10,50)/10,math.random(5,20),true,"Round","Add","Out",(root.CFrame-Vector3.new(0,2,0))*CFrame.Angles(math.rad(math.random(-5,5)),math.rad(math.random(-5,5)),math.rad(math.random(-5,5))),vt(0.01,0.01,0.01),math.random(50,800)/250,BrickColor.new("White"))
                local hitb = Instance.new("Part", char)
                    hitb.Anchored = true
                    hitb.CanCollide = false
                    hitb.FormFactor = 3
                    hitb.Name = "dont worry about it" --"Part"
                    hitb.Material = "Neon"
                    hitb.Size = Vector3.new(1, 1, 1)
                    hitb.Transparency = 1
                    hitb.TopSurface = 0
                    hitb.BottomSurface = 0
                hitb.CFrame = (root.CFrame + root.CFrame.lookVector*40*m)*CFrame.Angles(math.rad(90),0,0)
                for i = 0, 2 do
                    slash(math.random(30,50)/10,20,true,"Round","Add","Out",(hitb.CFrame)*CFrame.Angles(math.rad(math.random(-5,5)),math.rad(math.random(-5,5)),math.rad(math.random(-5,5))),vt(0.01,0.01,0.01),math.random(100*m,200*m)/250,BrickColor.new("White"))
                end
                for i = 0, 1 do
                    slash(math.random(5,20)/10,20,true,"Round","Add","Out",(hitb.CFrame)*CFrame.Angles(math.rad(math.random(-5,5)),math.rad(math.random(-5,5)),math.rad(math.random(-5,5))),vt(0.01,0.01,0.01),math.random(50*m,100*m)/250,BrickColor.new("White"))
                end
                symbolizeBlink(hitb,0,2109052855,Color3.new(1,1,1),5*m,0,0,0,root,false,0,1)
                local eff = Instance.new("ParticleEmitter",hitb)
                eff.Texture = "rbxassetid://2344870656"
                eff.LightEmission = 0.5
                eff.Color = ColorSequence.new(BrickColor.new("White").Color)
                eff.Rate = 200
                eff.Enabled = true
                eff.EmissionDirection = "Front"
