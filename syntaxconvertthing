        function PureBomb()
        if cooldowns then attack = true end
        hum.WalkSpeed =  0
        local orb = Instance.new("Part", char)
                orb.Anchored = true
                orb.BrickColor = MAINRUINCOLOR
                orb.CanCollide = false
                orb.FormFactor = 3
                orb.Name = "dont worry about it" --"Ring"
                orb.Material = "Neon"
                orb.Size = Vector3.new(1, 1, 1)
                orb.Transparency = 0
                orb.TopSurface = 0
                orb.BottomSurface = 0
                local orbm = Instance.new("SpecialMesh", orb)
                orbm.MeshType = "Sphere"
        orbm.Name = "dont worry about it" --"SizeMesh"
        orbm.Scale = vt(0,0,0)
        local scaled = 0.1
        local posid = 0
        CFuncs["Sound"].Create("rbxassetid://136007472", orb, 1,1.6)
        for i = 0, 3, 0.1 do
        swait()
        scaled = scaled - 0.002
        posid = posid - scaled
        orb.CFrame = rarm.CFrame*CFrame.new(0,-0.1+posid/1.05,0)
        local scaled = 0.1
        orbm.Scale = orbm.Scale + vt(scaled,scaled,scaled)
        sphereMKCharge(5,-0.25,"Add",orb.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),0.5,0.5,5,-0.005,MAINRUINCOLOR,10)
        PixelBlockNeg(2,1,"Add",orb.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),1,1,1,0.01,MAINRUINCOLOR,0)
        RH.C0=clerp(RH.C0,cf(1,-1 - 0.1 * math.cos(sine / 32),0)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(0),math.rad(0),math.rad(-2 - 1 * math.cos(sine / 32))),.1)
