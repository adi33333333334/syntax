        function SHDBullet()
            if cooldowns then attack = true end
        CFuncs["Sound"].Create("rbxassetid://136007472", rarm, 1.5,1.25)
        local cfor = CreateParta(char,1,1,"Neon",MAINRUINCOLOR)
        cfor.Anchored = true
        cfor.CFrame = cfor.CFrame
        local cef = Instance.new("ParticleEmitter",cfor)
        cef.Texture = "rbxassetid://2344870656"
        cef.LightEmission = 1
        cef.Color = ColorSequence.new(MAINRUINCOLOR.Color)
        cef.Rate = 150
        cef.Lifetime = NumberRange.new(0.25)
        cef.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,0,0),NumberSequenceKeypoint.new(0.5,1,0),NumberSequenceKeypoint.new(1,0,0)})
        cef.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.5,0.25,0),NumberSequenceKeypoint.new(1,1,0)})
        cef.Speed = NumberRange.new(0)
        local rval = 0
        local eval = 1
            for i = 0,3,0.3 do
        swait()
        rval = rval + math.random(30,40)
        eval = eval + 0.45
        cfor.Transparency = cfor.Transparency - 0.007
        cfor.Size = cfor.Size + vt(0.15,0.15,0.15)
        cef.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,0,0),NumberSequenceKeypoint.new(0.5,eval,0),NumberSequenceKeypoint.new(1,0,0)})
        cfor.CFrame = root.CFrame*CFrame.new(0,1,-7)*CFrame.Angles(math.rad(rval),math.rad(rval),math.rad(-rval))
        cfor.CFrame = cfor.CFrame
        sphere2(8,"Add",larm.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(1,1,1),-0.01,0.05,-0.01,MAINRUINCOLOR2,MAINRUINCOLOR2.Color)
        sphere2(10,"Add",larm.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(1,1,1),-0.01,0.15,-0.01,MAINRUINCOLOR,MAINRUINCOLOR.Color)
        RH.C0=clerp(RH.C0,cf(1,-0.4,-0.5)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-3),math.rad(0 - 1 * math.cos(sine / 56)),math.rad(-10 - 2 * math.cos(sine / 32))),.3)
        LH.C0=clerp(LH.C0,cf(-1,-1,0)*angles(math.rad(0),math.rad(-90),math.rad(0))*angles(math.rad(-3),math.rad(0 - 1 * math.cos(sine / 56)),math.rad(10 + 2 * math.cos(sine / 32))),.3)
        RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(0,0 + 0.02 * math.cos(sine / 32),1 + 0.15 * math.cos(sine / 32))*angles(math.rad(0 - 2 * math.cos(sine / 32)),math.rad(0),math.rad(-50)),.3)
        Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(10 - 2 * math.cos(sine / 37)),math.rad(10 + 1 * math.cos(sine / 58)),math.rad(50 + 2 * math.cos(sine / 53))),.3)
        RW.C0=clerp(RW.C0,cf(1.5,0.5 + 0.025 * math.cos(sine / 45),0)*angles(math.rad(10 + 6 * math.cos(sine / 72)),math.rad(3 - 2 * math.cos(sine / 58)),math.rad(5 + 2 * math.cos(sine / 45))),.3)
        LW.C0=clerp(LW.C0,cf(-1.5,0.5 + 0.025 * math.cos(sine / 45),0)*angles(math.rad(90 - 7 * math.cos(sine / 66)),math.rad(4 - 3 * math.cos(sine / 59)),math.rad(-50 - 4 * math.cos(sine / 45))),.3)
            end
            cef.Enabled = false
            coroutine.resume(coroutine.create(function()
            for i = 0,49 do
                swait()
                rval = rval + 100
                cfor.CFrame = cfor.CFrame*CFrame.Angles(math.rad(rval),math.rad(rval),math.rad(-rval))
                cfor.Transparency = cfor.Transparency + 0.02
                cfor.Size = cfor.Size + vt(5,5,5)
            end
            cfor:Destroy()
            cfor:Destroy()
        end))
            local lva = 1
            local ica = 0
        local cent = CreateParta(char,1,1,"Neon",MAINRUINCOLOR)
        CFuncs["Sound"].Create("rbxassetid://1177785010", cent, 10, 1)
        cent.CFrame = root.CFrame*CFrame.Angles(0,0,0) + root.CFrame.lookVector*5
        sphere2(2,"Add",cent.CFrame,vt(1,1,1),0.5,0.5,0.5,MAINRUINCOLOR2,MAINRUINCOLOR2.Color)
        sphere2(3,"Add",cent.CFrame,vt(1,1,1),0.5,0.5,0.5,MAINRUINCOLOR,MAINRUINCOLOR.Color)
        
        local a = Instance.new("Part",workspace)
            a.Name = "dont worry about it" --"Direction"	
            a.Anchored = true
            a.BrickColor = bc("Bright red")
        a.Material = "Neon"
        a.Transparency = 1
            a.CanCollide = false
            local ray = Ray.new(
                cent.CFrame.p,                           -- origin
                (script.Parent.mousehit.Value.p - cent.CFrame.p).unit * 500 -- direction
            ) 
            local ignore = cent
            local hit, position, normal = workspace:FindPartOnRay(ray, ignore)
            a.BottomSurface = 10
            a.TopSurface = 10
            local distance = (cent.CFrame.p - position).magnitude
            a.Size = Vector3.new(0.1, 0.1, 0.1)
            a.CFrame = CFrame.new(cent.CFrame.p, position) * CFrame.new(0, 0, 0)
        cent.CFrame = a.CFrame
        a:Destroy()
        local bv = Instance.new("BodyVelocity")
        bv.maxForce = Vector3.new(1e9, 1e9, 1e9)
        bv.Velocity = mult*cent.CFrame.lookVector*0
        bv.Parent = cent
        game:GetService("Debris"):AddItem(cent, 20)
        local hitted = false
        coroutine.resume(coroutine.create(function()
            swait(5)
            hitted = false
        end))
        coroutine.resume(coroutine.create(function()
            while true do
                swait(1)
                if hitted == false and cent.Parent ~= nil then
            ica = ica + 6*lva
            lva = lva + 0.1
            bv.Velocity = mult*cent.CFrame.lookVector*ica
            sphere2(3,"Add",cent.CFrame,vt(2.5,2.5,2.5),-0.05,-0.05,-0.05,MAINRUINCOLOR2)
            sphere2(5,"Add",cent.CFrame*CFrame.Angles(0,0,math.rad(ica))*CFrame.new(0,-5,0),vt(2,2,2),-0.04,-0.04,-0.04,MAINRUINCOLOR)
            sphere2(5,"Add",cent.CFrame*CFrame.Angles(0,0,math.rad(ica))*CFrame.new(0,5,0),vt(2,2,2),-0.04,-0.04,-0.04,MAINRUINCOLOR2)
                elseif hitted == true or cent.Parent == nil then
                    break
                end
            end
        end))
        coroutine.resume(coroutine.create(function()
        cent.Touched:connect(function(hit) 
            if hitted == false and hit.Parent ~= char then
            hitted = true
            cent.Anchored = true
            CFuncs["Sound"].Create("rbxassetid://782353443", cent, 5, 1.2)
            CFuncs["Sound"].Create("rbxassetid://1368637781", cent, 4, 1.2)
            CFuncs["Sound"].Create("rbxassetid://763717897", cent, 2.5, 1.2)
            CFuncs["EchoSound"].Create("rbxassetid://1177785010", cent, 8, 1.1,0,10,0.15,0.5,1)
            MagniDamage(cent, 50, 15,20, 0, "Normal")
            sphere2(2,"Add",cent.CFrame,vt(1,1,1),0.5,0.5,0.5,MAINRUINCOLOR2,MAINRUINCOLOR2.Color)
            sphere2(3,"Add",cent.CFrame,vt(1,1,1),0.6,0.6,0.6,MAINRUINCOLOR,MAINRUINCOLOR.Color)
            for i = 0, 9 do
                slash(math.random(10,50)/10,5,true,"Round","Add","Out",cent.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.01,0.01,0.01),math.random(100,200)/250,MAINRUINCOLOR2)
                slash(math.random(10,50)/10,5,true,"Round","Add","Out",cent.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.01,0.01,0.01),math.random(75,150)/250,MAINRUINCOLOR)
            end
        end
        end)
        end))
        attack = false
        hum.WalkSpeed = speed*storeHumanoidWS
        end
        
        function SHDRain()
            if cooldowns then attack = true end
        hum.WalkSpeed =  2
        local radm = math.random(1,3)
        if radm == 1 then
        bosschatfunc("Here comes something really big!",MAINRUINCOLOR.Color,1)
        elseif radm == 2 then
        bosschatfunc("Plasmatic Burst: Shining Salvo!",MAINRUINCOLOR.Color,1)
        elseif radm == 3 then
        bosschatfunc("Submit to the brightness!",MAINRUINCOLOR.Color,1)
        end
        hum.WalkSpeed =  0
        for i = 0, 10, 0.1 do
        swait()
        PixelBlockNeg(1,math.random(1,2),"Add",sorb.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),1,1,1,0.02,MAINRUINCOLOR,0)
        RH.C0=clerp(RH.C0,cf(1,-1 - 0.1 * math.cos(sine / 32),0)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(0),math.rad(0),math.rad(-2 - 1 * math.cos(sine / 32))),.4)
        LH.C0=clerp(LH.C0,cf(-1,-1 - 0.1 * math.cos(sine / 32),0)*angles(math.rad(0),math.rad(-90),math.rad(0))*angles(math.rad(-3 + 1 * math.cos(sine / 32)),math.rad(0),math.rad(0)),.4)
        RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(0,0,0 + 0.1 * math.cos(sine / 32))*angles(math.rad(0),math.rad(0),math.rad(40)),.4)
        Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(20),math.rad(0),math.rad(-40)),.4)
        RW.C0=clerp(RW.C0,cf(1.5,0.5,0)*angles(math.rad(170),math.rad(0),math.rad(0)),.4)
