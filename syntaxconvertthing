            end
            if rul2 ~= nil then
                rul2:BreakJoints()
                local rulc = Instance.new("Glue",t2) rulc.Name = "dont worry about it" --"yo" rulc.C0 = CFrame.new(lt2.Size.x/4,-lt2.Size.y/2,0) rulc.C1 = CFrame.new(0,rul2.Size.y/3,0) rulc.Part0 = lt2 rulc.Part1 = rul2
                local rulcc = Instance.new("Part",t2) rulcc.Name = "dont worry about it" --"yo" rulcc.Transparency = trans rulcc.Size = Vector3.new(rul2.Size.x/1.2,rul2.Size.y/4,rul2.Size.z/1.2)
