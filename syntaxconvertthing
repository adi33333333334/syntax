            sphere2(3,"Add",cent.CFrame,vt(1,1,1),1.2,1.2,1.2,MAINRUINCOLOR,MAINRUINCOLOR.Color)
            for i = 0, 19 do
                slash(math.random(10,50)/10,5,true,"Round","Add","Out",cent.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.01,0.01,0.01),math.random(200,400)/250,MAINRUINCOLOR)
                slash(math.random(10,50)/10,5,true,"Round","Add","Out",cent.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0.01,0.01,0.01),math.random(150,300)/250,MAINRUINCOLOR2)
            end
