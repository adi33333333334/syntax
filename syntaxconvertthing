        obj1.CFrame = root.CFrame*CFrame.new(0,1,-5)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(rval))
            end
        end))
        for i = 0,5,0.1 do
        swait()
        if rainbowmode == true then
        orbe.Color = modet.TextColor3
