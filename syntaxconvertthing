                local rhcw = Instance.new("Weld",t2) rhcw.Name = "dont worry about it" --"yo" rhcw.Part0 = rh2 rhcw.Part1 = rhcc rhcw.C0 = CFrame.new(0,0,0)
            end
            if lh2 ~= nil then
                lh2:BreakJoints()
                local lhc = Instance.new("Glue",t2) lhc.Name = "dont worry about it" --"yo" lhc.C0 = CFrame.new(0,-lla2.Size.y/2,0) lhc.C1 = CFrame.new(0,lh2.Size.y/3,0) lhc.Part0 = lla2 lhc.Part1 = lh2
                local lhcc = Instance.new("Part",t2) lhcc.Name = "dont worry about it" --"yo" lhcc.Transparency = trans lhcc.Size = Vector3.new(lh2.Size.x/1.2,lh2.Size.y,lh2.Size.z/1.2)
