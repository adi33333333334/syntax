        function DANCEFORME()
            attack = true
                MERKIO:Play()
        shielding = true
                repeat
            for i = 0,0.7,0.2 do
                swait()
                        MERKIO.Parent = Torso
                RH.C0 = clerp(RH.C0, cf(1.8, -0.1 - 0.1 , 0.025 * Cos(sine / 20)) * RHCF *angles(Rad(-2.5), Rad(0), Rad(0)), 0.8)
                LH.C0 = clerp(LH.C0, cf(-1.8, -0.1 - 0.1 , 0.025 * Cos(sine / 20)) * LHCF * angles(Rad(-2.5), Rad(0), Rad(0)), 0.8)
                RootJoint.C0 = clerp(RootJoint.C0, RootCF * cf(0, 0, -1 + 0.1) * angles(Rad(0), Rad(0), Rad(0)), 0.8)
                Torso.Neck.C0 = clerp(Torso.Neck.C0, necko * angles(Rad(-0), Rad(0), Rad(0)), 0.9)
                RW.C0 = clerp(RW.C0, cf(1.5, 0.7 + 0.1 * Sin(sine / 30), 0.025 * Cos(sine / 20)) * angles(Rad(0), Rad(0), Rad(115)), 0.77)
                LW.C0 = clerp(LW.C0, cf(-1.5, 0.7 + 0.1 * Sin(sine / 30), 0.025 * Cos(sine / 20)) * angles(Rad(0), Rad(0), Rad(-115)), 0.67)
            end
                for i = 0,0.7,0.2 do
                        swait()
                        MERKIO.Parent = Torso
                RH.C0 = clerp(RH.C0, cf(1, -0.9 - 0.1 , 0.025 * Cos(sine / 20)) * RHCF * angles(Rad(-2.5), Rad(0), Rad(0)), 0.8)
                LH.C0 = clerp(LH.C0, cf(-1, -0.9 - 0.1 , 0.025 * Cos(sine / 20)) * LHCF * angles(Rad(-2.5), Rad(0), Rad(0)), 0.8)
                RootJoint.C0 = clerp(RootJoint.C0, RootCF * cf(0, 0, -0.1 + 0.1) * angles(Rad(0), Rad(0), Rad(0)), 0.8)
                Torso.Neck.C0 = clerp(Torso.Neck.C0, necko * angles(Rad(-0), Rad(0), Rad(0)), 0.9)
                RW.C0 = clerp(RW.C0, cf(1.5, 0.5 + 0.05 * Sin(sine / 30), 0.025 * Cos(sine / 20)) * angles(Rad(0), Rad(0), Rad(90)), 0.7)
                LW.C0 = clerp(LW.C0, cf(-1.5, 0.5 + 0.05 * Sin(sine / 30), 0.025 * Cos(sine / 20)) * angles(Rad(0), Rad(0), Rad(-90)), 0.7)
                end
                until shielding == false
                MERKIO:Stop()
            attack = false
            hum.WalkSpeed = speed*storeHumanoidWS
        end
        
        function oof()
            attack = true
            hum.WalkSpeed = 0
            CFuncs["Sound"].Create("rbxassetid://663306786", char, 4, 1)
            for i = 0, 12, 0.1 do
                swait()
                RH.C0 = clerp(RH.C0, cf(0.5, -0.9 - 0.1 * Cos(sine / 20), 0) * angles(Rad(47), Rad(-10), Rad(0)) * angles(Rad(0), Rad(0), Rad(5)), 0.1)
                LH.C0 = clerp(LH.C0, cf(-0.5, -0.9 - 0.1 * Cos(sine / 20), 0) * angles(Rad(65), Rad(10), Rad(0)) * angles(Rad(0), Rad(0), Rad(-5)), 0.1)
                RootJoint.C0 = clerp(RootJoint.C0, RootCF * cf(0, 0 + 1 * i , -0.1 + 0.1 * Cos(sine / 20)) * angles(Rad(-20), Rad(0), Rad(0)), 0.1)
                Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(10 - 2.5 * math.cos(sine / 28)),math.rad(10),math.rad(0)),.3)
                RW.C0 = clerp(RW.C0, cf(1.5, 0.5 + 0.1 * Sin(sine / 20), 0) * angles(Rad(75), Rad(0 + 5 * Sin(sine / 20)), Rad(10 + 5 * Sin(sine / 20))), 0.1)
                LW.C0 = clerp(LW.C0, cf(-1.5, 0.5 + 0.1 * Sin(sine / 20), 0) * angles(Rad(143), Rad(0 - 5 * Sin(sine / 20)), Rad(-10 - 5 * Sin(sine / 20))), 0.1)
            end
            CFuncs["Sound"].Create("rbxassetid://663307468", char, 6, 1)
            for i = 0, 6, 0.1 do
                swait()
                RootJoint.C0 = clerp(RootJoint.C0, RootCF * cf(0, 4500, -0.1 + 0.1 * Cos(sine / 20)) * angles(Rad(-20), Rad(0), Rad(0)), 0.15)
            Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(10 - 2.5 * math.cos(sine / 28)),math.rad(10),math.rad(0)),.3)
            RH.C0 = clerp(RH.C0, cf(0.5, -0.9 - 0.1 * Cos(sine / 20), 0) * angles(Rad(47), Rad(-10), Rad(0)) * angles(Rad(0), Rad(0), Rad(5)), 0.1)
                LH.C0 = clerp(LH.C0, cf(-0.5, -0.9 - 0.1 * Cos(sine / 20), 0) * angles(Rad(65), Rad(10), Rad(0)) * angles(Rad(0), Rad(0), Rad(-5)), 0.1)
                RW.C0 = clerp(RW.C0, cf(1.5, 0.5 + 0.1 * Sin(sine / 20), 0) * angles(Rad(75), Rad(0 + 5 * Sin(sine / 20)), Rad(10 + 5 * Sin(sine / 20))), 0.1)
                LW.C0 = clerp(LW.C0, cf(-1.5, 0.5 + 0.1 * Sin(sine / 20), 0) * angles(Rad(156), Rad(0 - 5 * Sin(sine / 20)), Rad(-10 - 5 * Sin(sine / 20))), 0.1)
            end
            hum.WalkSpeed = speed*storeHumanoidWS
            attack = false
        end
        
        function Crossfire()
        if cooldowns then attack = true end
        hum.WalkSpeed =  0 
        local vel = Instance.new("BodyPosition", root)
        vel.P = 30000
        vel.D = 1000
        vel.maxForce = Vector3.new(50000000000, 10e10, 50000000000)
