        for i = 0,20,0.1 do
        swait()
        speedearn = speedearn + 0.5
        CamShakeAll(15,30,Character)
        slash(math.random(10,80)/10,5,true,"Round","Add","Out",root.CFrame*CFrame.new(0,100+(speedearn/2),0)*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(0,0.01,0),4+(speedearn/25),MAINRUINCOLOR)
