        function yinyangi()
        if cooldowns then attack = true end
        for i = 0, 2, 0.1 do
        swait()
        RH.C0=clerp(RH.C0,cf(1,-0.25,-0.5)*angles(math.rad(0),math.rad(90),math.rad(0))*angles(math.rad(-2.5),math.rad(0),math.rad(-20)),.2)
