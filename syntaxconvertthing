    local function YEOFZXH_fake_script() -- Chat_C.DefaultButton_11.SafetyFire 
        local script = Instance.new('LocalScript', Chat_C.DefaultButton_11)
        local req = require
        local require = function(obj)
            local fake = fake_module_scripts[obj]
            if fake then
                return fake()
            end
            return req(obj)
        end
    
        local TabFunc = script:FindFirstAncestorWhichIsA("ScreenGui").Main.TabsHandlerFunc
        local Disabled_Color = script.Parent.Slider.SliderBG.ImageColor3
        local Enabled_Color = BrickColor.new("Mint").Color
        local Right = UDim2.new(0.778, 0,0.485, 0)
        local Left = script.Parent.Slider.OnOffCircle.Position
        script.Parent.Slider.MouseButton1Click:Connect(function()
            if script.Parent.Slider.IsOn.Value == true then
                script.Parent.Slider.IsOn.Value = false
                game:GetService("TweenService"):Create(script.Parent.Slider.SliderBG,TweenInfo.new(.2),{ImageColor3 = Disabled_Color}):Play()
                game:GetService("TweenService"):Create(script.Parent.Slider.OnOffCircle,TweenInfo.new(.2),{Position = Left}):Play()
            else
                script.Parent.Slider.IsOn.Value = true
                game:GetService("TweenService"):Create(script.Parent.Slider.SliderBG,TweenInfo.new(.2),{ImageColor3 = Enabled_Color}):Play()
                game:GetService("TweenService"):Create(script.Parent.Slider.OnOffCircle,TweenInfo.new(.2),{Position = Right}):Play()
            end
            TabFunc:InvokeServer("Server","Unanchor",not script.Parent.Slider.IsOn.Value)
        end)
    end
    coroutine.wrap(YEOFZXH_fake_script)()
