        end
        
        blinkstale = 0
        
        function Blink()
            if cooldowns then attack = true end
        for i = 0, 10 do
        sphereMK(3,math.random(1,8)/10,"Add",root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),1,1,20,0,MAINRUINCOLOR,0)
        end
        sphere(10,"Add",root.CFrame,vt(0,0,0),1,MAINRUINCOLOR)
        RootPart.CFrame = script.Parent.mousehit.Value * CFrame.new(0,3,0)
        CameraEnshaking(2,10)
        CFuncs["Sound"].Create("rbxassetid://2943064526", root, 1,1.5)
        for i = 0, 10 do
        sphereMK(3,math.random(1,8)/10,"Add",root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),1,1,20,0,MAINRUINCOLOR,0)
        end
        sphere(10,"Add",root.CFrame,vt(0,0,0),1,MAINRUINCOLOR)
        blinkstale = blinkstale + 3
        blinkstale = blinkstale * 1.5
        coroutine.resume(coroutine.create(function()
            local heggk = blinkstale
            swait(300)
            if blinkstale == heggk then
                blinkstale = 0
            end
        end))
        hum.WalkSpeed =  2
        swait(blinkstale)
        hum.WalkSpeed = speed*storeHumanoidWS
        attack = false
        end
        
        function TearingSpace()
            if cooldowns then attack = true end
            hum.WalkSpeed =  5
