        CFuncs["Sound"].Create("rbxassetid://824687369", xb, 1,0.75)
        CFuncs["Sound"].Create("rbxassetid://822968467", xb, 2,0.95)
        CFuncs["Sound"].Create("rbxassetid://822969951", xb, 3,1)
        CFuncs["Sound"].Create("rbxassetid://824687369", xc, 1,0.75)
