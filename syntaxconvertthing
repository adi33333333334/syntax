        local cen = CreateParta(m,1,1,"SmoothPlastic",BrickColor.random())
        CreateWeld(cen,root,cen,0,3,0,math.rad(0),math.rad(0),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))
