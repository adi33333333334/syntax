                eff.Speed = NumberRange.new(5,20)
                eff.Drag = 5
                eff.Rotation = NumberRange.new(-500,500)
                eff.VelocitySpread = 9000
                eff.RotSpeed = NumberRange.new(-50,50)
                swait(60)
                eff:Destroy()
                end))
                swait(45)
                CFuncs["Sound"].Create("rbxassetid://2162239324", hitb, 10, 0.75)
                for i = 1,10 do
