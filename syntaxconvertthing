        a.Transparency = 1
            a.CanCollide = false
            local ray = Ray.new(
                cent.CFrame.p,                           -- origin
                (script.Parent.mousehit.Value.p - cent.CFrame.p).unit * 500 -- direction
            ) 
            local ignore = cent
            local hit, position, normal = workspace:FindPartOnRay(ray, ignore)
            a.BottomSurface = 10
            a.TopSurface = 10
            local distance = (cent.CFrame.p - position).magnitude
            a.Size = Vector3.new(0.1, 0.1, 0.1)
